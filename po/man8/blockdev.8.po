#. type: TH
#, no-wrap
msgid "BLOCKDEV"
msgstr "BLOCKDEV"

#. type: TH
#, no-wrap
msgid "August 2010"
msgstr ""

#. type: TH
#, no-wrap
msgid "util-linux"
msgstr ""

#. type: TH
#, no-wrap
msgid "System Administration"
msgstr ""

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
msgid "blockdev - call block device ioctls from the command line"
msgstr ""

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
msgid ""
"B<blockdev> [B<-q>] [B<-v>] I<command> [I<command>...] I<device> "
"[I<device>...]"
msgstr ""

#. type: Plain text
msgid "B<blockdev> B<--report> [I<device>...]"
msgstr ""

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
msgid ""
"The utility B<blockdev> allows one to call block device ioctls from the "
"command line."
msgstr ""

#. type: SH
#, no-wrap
msgid "OPTIONS"
msgstr "OPTIONEN"

#. type: IP
#, no-wrap
msgid "B<-V>"
msgstr "B<-V>"

#. type: Plain text
msgid "Print version and exit."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<-q>"
msgstr "B<-q>"

#. type: Plain text
msgid "Be quiet."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<-v>"
msgstr "B<-v>"

#. type: Plain text
msgid "Be verbose."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--report>"
msgstr ""

#. type: Plain text
msgid ""
"Print a report for the specified device. It is possible to give multiple "
"devices. If none is given, all devices which appear in /proc/partitions are "
"shown. Note that the partition StartSec is in 512-byte sectors."
msgstr ""

#. type: SH
#, no-wrap
msgid "COMMANDS"
msgstr ""

#. type: Plain text
msgid "It is possible to give multiple devices and multiple commands."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--flushbufs>"
msgstr ""

#. type: Plain text
msgid "Flush buffers."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getalignoff>"
msgstr ""

#. type: Plain text
msgid "Get alignment offset."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getbsz>"
msgstr ""

#. type: Plain text
msgid "Print blocksize in bytes."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getdiscardzeroes>"
msgstr ""

#. type: Plain text
msgid "Get discard zeroes support status."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getfra>"
msgstr ""

#. type: Plain text
msgid "Get filesystem readahead in 512-byte sectors."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getiomin>"
msgstr ""

#. type: Plain text
msgid "Get minimum I/O size."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getioopt>"
msgstr ""

#. type: Plain text
msgid "Get optimal I/O size."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getmaxsect>"
msgstr ""

#. type: Plain text
msgid "Get max sectors per request"
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getpbsz>"
msgstr ""

#. type: Plain text
msgid "Get physical block (sector) size."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getra>"
msgstr ""

#. type: Plain text
msgid "Print readahead (in 512-byte sectors)."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getro>"
msgstr ""

#. type: Plain text
msgid "Get read-only. Print 1 if the device is read-only, 0 otherwise."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getsize64>"
msgstr ""

#. type: Plain text
msgid "Print device size in bytes."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getsize>"
msgstr ""

#. type: Plain text
msgid ""
"Print device size (32-bit!) in sectors. Deprecated in favor of the --getsz "
"option."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getss>"
msgstr ""

#. type: Plain text
msgid "Print sectorsize in bytes - usually 512."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--getsz>"
msgstr ""

#. type: Plain text
msgid "Get size in 512-byte sectors."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--rereadpt>"
msgstr ""

#. type: Plain text
msgid "Reread partition table"
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--setbsz> I<bytes>"
msgstr ""

#. type: Plain text
msgid "Set blocksize."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--setfra> I<sectors>"
msgstr ""

#. type: Plain text
