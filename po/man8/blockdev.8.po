msgstr ""

#. type: IP
#, no-wrap
msgid "B<--setra> I<sectors>"
msgstr ""

#. type: Plain text
msgid "Set readahead (in 512-byte sectors)."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--setro>"
msgstr ""

#. type: Plain text
