msgid "This command doesn't manipulate partitions on hard drive."
msgstr ""

#. type: SH
#, no-wrap
msgid "PARAMETERS"
msgstr ""

#. type: TP
#, no-wrap
msgid "I<device>"
msgstr ""

#. type: Plain text
