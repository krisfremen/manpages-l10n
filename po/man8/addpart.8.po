msgstr ""

#. type: TP
#, no-wrap
msgid "I<partition>"
msgstr ""

#. type: Plain text
msgid "Specify the partition number."
msgstr ""

#. type: TP
#, no-wrap
msgid "I<start>"
msgstr ""

#. type: Plain text
