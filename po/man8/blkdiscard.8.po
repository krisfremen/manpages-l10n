msgid "BLKDISCARD"
msgstr "BLKDISCARD"

#. type: TH
#, no-wrap
msgid "July 2014"
msgstr ""

#. type: TH
#, no-wrap
msgid "util-linux"
msgstr ""

#. type: TH
#, no-wrap
msgid "System Administration"
