#. type: SH
#: archlinux debian-unstable
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: archlinux debian-unstable
msgid "/etc/systemd/journald\\&.conf"
msgstr "/etc/systemd/journald\\&.conf"

#. type: Plain text
#: archlinux debian-unstable
msgid "/etc/systemd/journald\\&.conf\\&.d/*\\&.conf"
msgstr "/etc/systemd/journald\\&.conf\\&.d/*\\&.conf"

