#. type: TH
#: opensuse-leap-15-3
#, no-wrap
msgid "2017-09-15"
msgstr "15 september 2017"

#. type: Plain text
#: opensuse-leap-15-3
#, no-wrap
msgid ""
"    /* Obtain a duplicate of 'newfd' that can subsequently\n"
"       be used to check for close() errors; an EBADF error\n"
"       means that 'newfd' was not open. */\n"
msgstr ""

#. type: Plain text
#: opensuse-leap-15-3
#, no-wrap
msgid ""
"    tmpfd = dup(newfd);\n"
"    if (tmpfd == -1 && errno != EBADF) {\n"
"        /* Handle unexpected dup() error */\n"
"    }\n"
msgstr ""

#. type: Plain text
#: opensuse-leap-15-3
#, no-wrap
msgid "    /* Atomically duplicate 'oldfd' on 'newfd' */\n"
msgstr ""

#. type: Plain text
#: opensuse-leap-15-3
#, no-wrap
msgid ""
"    if (dup2(oldfd, newfd) == -1) {\n"
"        /* Handle dup2() error */\n"
"    }\n"
msgstr ""

#. type: Plain text
#: opensuse-leap-15-3
#, no-wrap
msgid ""
"    /* Now check for close() errors on the file originally\n"
"       referred to by 'newfd' */\n"
msgstr ""

#. type: Plain text
#: opensuse-leap-15-3
#, no-wrap
msgid ""
"    if (tmpfd != -1) {\n"
"        if (close(tmpfd) == -1) {\n"
"            /* Handle errors from close */\n"
"        }\n"
"    }\n"
msgstr ""

#. type: Plain text
#: opensuse-leap-15-3
msgid "B<close>(2), B<fcntl>(2), B<open>(2)"
msgstr "B<close>(2), B<fcntl>(2), B<open>(2)"

