#. type: Plain text
msgid "operator - C operator precedence and order of evaluation"
msgstr "operator - Rangfolge der C-Operatoren und Reihenfolge der Auswertung"

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
msgid "This manual page lists C operators and their precedence in evaluation."
msgstr ""
"Diese Handbuchseite listet C-Operatoren und ihre Rangfolge bei der "
"Auswertung."

#. type: Plain text
#, no-wrap
msgid ""
"B<Operator                             Associativity>\n"
"() [] -E<gt> .                           left to right\n"
"! ~ ++ -- + - (type) * & sizeof      right to left\n"
"* / %                                left to right\n"
"+ -                                  left to right\n"
"E<lt>E<lt> E<gt>E<gt>                                left to right\n"
"E<lt> E<lt>= E<gt> E<gt>=                            left to right\n"
"== !=                                left to right\n"
"&                                    left to right\n"
"^                                    left to right\n"
"|                                    left to right\n"
"&&                                   left to right\n"
"||                                   left to right\n"
"?:                                   right to left\n"
"= += -= *= /= %= E<lt>E<lt>= E<gt>E<gt>= &= ^= |=    right to left\n"
",                                    left to right\n"
msgstr ""
"B<Operator                             Assoziativität>\n"
"() [] -E<gt> .                           von links nach rechts\n"
"! ~ ++ -- + - (type) * & sizeof      von rechts nach links\n"
"* / %                                von links nach rechts\n"
"+ -                                  von links nach rechts\n"
"E<lt>E<lt> E<gt>E<gt>                                von links nach rechts\n"
"E<lt> E<lt>= E<gt> E<gt>=                            von links nach rechts\n"
"== !=                                von links nach rechts\n"
"&                                    von links nach rechts\n"
"^                                    von links nach rechts\n"
"|                                    von links nach rechts\n"
"&&                                   von links nach rechts\n"
"||                                   von links nach rechts\n"
"?:                                   von rechts nach links\n"
"= += -= *= /= %= E<lt>E<lt>= E<gt>E<gt>= &= ^= |=    von rechts nach links\n"
",                                    von links nach rechts\n"

