msgid "WAIT4"
msgstr "WAIT4"

#. type: TH
#, no-wrap
msgid "2010-09-20"
msgstr "20. September 2010"

#. type: TH
#, no-wrap
msgid "Linux"
msgstr "Linux"

#. type: TH
#, no-wrap
