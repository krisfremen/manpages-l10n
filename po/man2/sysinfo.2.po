msgid "SYSINFO"
msgstr "SYSINFO"

#. type: TH
#, no-wrap
msgid "2007-11-15"
msgstr "15. November 2007"

#. type: TH
#, no-wrap
msgid "Linux"
msgstr "Linux"

#. type: TH
#, no-wrap
