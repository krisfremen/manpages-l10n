msgid "SETFSGID"
msgstr "SETFSGID"

#. type: TH
#, no-wrap
msgid "2013-08-08"
msgstr "8. August 2013"

#. type: TH
#, no-wrap
msgid "Linux"
msgstr "Linux"

#. type: TH
#, no-wrap
