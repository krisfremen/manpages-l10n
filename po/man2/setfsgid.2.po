msgstr "ÜBERSICHT"

#. type: Plain text
#: ../english/manpages-dev/man2/setfsgid.2:35
msgid "B<#include E<lt>unistd.hE<gt>> /* glibc uses E<lt>sys/fsuid.hE<gt> */"
msgstr ""

