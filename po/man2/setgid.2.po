msgid "SETGID"
msgstr "SETGID"

#. type: TH
#, no-wrap
msgid "2016-10-08"
msgstr "8. Oktober 2016"

#. type: TH
#, no-wrap
msgid "Linux"
msgstr "Linux"

#. type: TH
#, no-wrap
