#. type: TH
#, no-wrap
msgid "SHMGET"
msgstr "SHMGET"

#. type: TH
#, no-wrap
msgid "2006-05-02"
msgstr "2. Mai 2006"

#. type: TH
#, no-wrap
msgid "Linux"
msgstr "Linux"

#. type: TH
