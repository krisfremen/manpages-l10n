#. type: Plain text
msgid "statfs, fstatfs - get file system statistics"
msgstr "statfs, fstatfs - Dateisystemstatistiken ermitteln"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
msgid "B<#include E<lt>sys/vfs.hE<gt> >/* or E<lt>sys/statfs.hE<gt> */"
msgstr "B<#include E<lt>sys/vfs.hE<gt> >/* oder E<lt>sys/statfs.hE<gt> */"

#. type: Plain text
msgid "B<int statfs(const char *>I<path>B<, struct statfs *>I<buf>B<);>"
msgstr "B<int statfs(const char *>I<path>B<, struct statfs *>I<buf>B<);>"

#. type: Plain text
msgid "B<int fstatfs(int >I<fd>B<, struct statfs *>I<buf>B<);>"
msgstr "B<int fstatfs(int >I<fd>B<, struct statfs *>I<buf>B<);>"

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
msgid ""
"The function B<statfs>()  returns information about a mounted file system.  "
"I<path> is the pathname of any file within the mounted file system.  I<buf> "
"is a pointer to a I<statfs> structure defined approximately as follows:"
msgstr ""
"Die Funktion B<statfs>() gibt Informationen über ein eingehängtes "
"Dateisystem zurück. I<Path> ist der Pfadname einer Datei in dem eingehängten "
"Dateisystem. I<buf> ist ein Zeiger auf eine Struktur I<statfs>, die ungefähr "
"wie folgt definiert ist:"

#. type: Plain text
#, no-wrap
msgid ""
"#if __WORDSIZE == 32\t\t/* System word size */\n"
"# define __SWORD_TYPE           int\n"
"#else /* __WORDSIZE == 64 */\n"
"# define __SWORD_TYPE\t\tlong int\n"
"#endif\n"
msgstr ""
"#if __WORDSIZE == 32\t\t/* System-Wordgröße */\n"
"# define __SWORD_TYPE           int\n"
"#else /* __WORDSIZE == 64 */\n"
"# define __SWORD_TYPE\t\tlong int\n"
"#endif\n"

#. type: Plain text
#, no-wrap
msgid ""
"struct statfs {\n"
"    __SWORD_TYPE f_type;    /* type of file system (see below) */\n"
"    __SWORD_TYPE f_bsize;   /* optimal transfer block size */\n"
"    fsblkcnt_t   f_blocks;  /* total data blocks in file system */\n"
"    fsblkcnt_t   f_bfree;   /* free blocks in fs */\n"
"    fsblkcnt_t   f_bavail;  /* free blocks available to\n"
"                               unprivileged user */\n"
"    fsfilcnt_t   f_files;   /* total file nodes in file system */\n"
"    fsfilcnt_t   f_ffree;   /* free file nodes in fs */\n"
"    fsid_t       f_fsid;    /* file system id */\n"
"    __SWORD_TYPE f_namelen; /* maximum length of filenames */\n"
"    __SWORD_TYPE f_frsize;  /* fragment size (since Linux 2.6) */\n"
"    __SWORD_TYPE f_spare[5];\n"
"};\n"
msgstr ""
"struct statfs {\n"
"    __SWORD_TYPE f_type;    /* Typ des Dateisystems (siehe unten) */\n"
"    __SWORD_TYPE f_bsize;   /* optimale übertragungsblockgröße */\n"
"    fsblkcnt_t   f_blocks;  /* gesamte Datenblöcke im Dateisystem */\n"
"    fsblkcnt_t   f_bfree;   /* freie Blöcke im Dateisystem */\n"
"    fsblkcnt_t   f_bavail;  /* freie Blöcke verfügbar für nicht-Superuser */\n"
"    fsfilcnt_t   f_files;   /* gesamte Dateiknoten im Dateisystem */\n"
"    fsfilcnt_t   f_ffree;   /* freie Dateiknoten im Dateisystem */\n"
"    fsid_t       f_fsid;    /* Dateisystemkennung */\n"
"    __SWORD_TYPE f_namelen; /* maximale Dateinamenlänge */\n"
"    __SWORD_TYPE f_frsize;  /* Fragmentgröße (seit Linux 2.6) */\n"
"    __SWORD_TYPE f_spare[5];\n"
"};\n"

