#. type: Plain text
#, fuzzy
msgid "sysconf - Get configuration information at runtime"
msgstr "sysconf - hole Konfigurationsinformationen zur Laufzeit"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#, no-wrap
msgid "B<#include E<lt>unistd.hE<gt>>\n"
msgstr "B<#include E<lt>unistd.hE<gt>>\n"

#. type: Plain text
#, fuzzy, no-wrap
msgid "B<long sysconf(int >I<name>B<);>\n"
