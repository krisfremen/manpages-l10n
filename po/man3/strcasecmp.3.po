#, fuzzy
msgid ""
"B<bcmp>(3), B<memcmp>(3), B<strcmp>(3), B<strcoll>(3), B<strncmp>(3), "
"B<wcscasecmp>(3), B<wcsncasecmp>(3)"
msgstr "B<bcmp>(3), B<memcmp>(3), B<strcmp>(3), B<strcoll>(3), B<strncmp>(3)."

