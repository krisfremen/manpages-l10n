msgstr "B<int isblank(int >I<c>B<);>\n"

#. type: Plain text
#, no-wrap
msgid "B<int iscntrl(int >I<c>B<);>\n"
msgstr "B<int iscntrl(int >I<c>B<);>\n"

#. type: Plain text
