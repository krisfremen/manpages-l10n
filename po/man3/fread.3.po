msgid "FREAD"
msgstr "FREAD"

#. type: TH
#, no-wrap
msgid "1996-05-17"
msgstr "17. Mai 1996"

#. type: TH
#, no-wrap
msgid "GNU"
msgstr "GNU"

#. type: TH
#, no-wrap
