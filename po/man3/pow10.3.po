#. type: TH
#, no-wrap
msgid "POW10"
msgstr "POW10"

#. type: TH
#, no-wrap
msgid "2015-03-02"
msgstr "2. März 2015"

#. type: TH
#, no-wrap
msgid "Linux Programmer's Manual"
msgstr "Linux-Programmierhandbuch"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
msgid "pow10, pow10f, pow10l - base-10 power functions"
msgstr "pow10, pow10f, pow10l - Exponentialfunktionen mit der Basis 10"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#, no-wrap
msgid ""
"B<#define _GNU_SOURCE>         /* See feature_test_macros(7) */\n"
"B<#include E<lt>math.hE<gt>>\n"
msgstr ""
"B<#define _GNU_SOURCE>         /* Siehe feature_test_macros(7) */\n"
"B<#include E<lt>math.hE<gt>>\n"

#. type: Plain text
#, no-wrap
msgid "B<double pow10(double >I<x>B<);>\n"
msgstr "B<double pow10(double >I<x>B<);>\n"

#. type: Plain text
#, no-wrap
msgid "B<float pow10f(float >I<x>B<);>\n"
msgstr "B<float pow10f(float >I<x>B<);>\n"

#. type: Plain text
#, no-wrap
msgid "B<long double pow10l(long double >I<x>B<);>\n"
msgstr "B<long double pow10l(long double >I<x>B<);>\n"

#. type: Plain text
msgid "Link with I<-lm>."
msgstr "Linken Sie mit der Option I<-lm>."

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
msgid "These functions return the value of 10 raised to the power I<x>."
msgstr "Diese Funktionen geben die I<x>-te Potenz von 10 zurück."

#. type: SH
#, no-wrap
msgid "VERSIONS"
msgstr "VERSIONEN"

#. type: Plain text
msgid "These functions first appeared in glibc in version 2.1."
msgstr "Diese Funktionen kamen erstmals in Glibc 2.1 vor."

#. type: SH
#, no-wrap
msgid "ATTRIBUTES"
msgstr "ATTRIBUTE"

#. type: Plain text
msgid ""
"For an explanation of the terms used in this section, see B<attributes>(7)."
msgstr ""
"Siehe B<attributes>(7) für eine Erläuterung der in diesem Abschnitt "
"verwandten Ausdrücke."

#. type: tbl table
#, no-wrap
msgid "Interface"
msgstr "Schnittstelle"

#. type: tbl table
#, no-wrap
msgid "Attribute"
msgstr "Attribut"

#. type: tbl table
#, no-wrap
msgid "Value"
msgstr "Wert"

#. type: tbl table
#, no-wrap
msgid ""
"B<pow10>(),\n"
"B<pow10f>(),\n"
"B<pow10l>()\n"
msgstr ""
"B<pow10>(),\n"
"B<pow10f>(),\n"
"B<pow10l>()\n"

#. type: tbl table
#, no-wrap
msgid "Thread safety"
msgstr "Multithread-Fähigkeit"

#. type: tbl table
#, no-wrap
msgid "MT-Safe"
msgstr "MT-fähig"

#. type: SH
#, no-wrap
msgid "CONFORMING TO"
msgstr "KONFORM ZU"

#. type: Plain text
msgid "This is a GNU extension."
msgstr "Dies ist eine GNU-Erweiterung."

#. type: SH
#, no-wrap
msgid "NOTES"
msgstr "ANMERKUNGEN"

#. type: Plain text
msgid ""
"These functions are identical to the functions listed under B<exp10>(3)."
msgstr ""
"Diese Funktionen sind identisch mit den unter B<exp10>(3) aufgelisteten."

#. type: SH
#, no-wrap
msgid "SEE ALSO"
msgstr "SIEHE AUCH"

#. type: Plain text
msgid "B<exp10>(3), B<pow>(3)"
msgstr "B<exp10>(3), B<pow>(3)"

#. type: SH
#, no-wrap
msgid "COLOPHON"
msgstr "KOLOPHON"

