msgid ""
"getdate, getdate_r - convert a date-plus-time string to broken-down time"
msgstr "getdate - Berechnet aus einer Zeichenkette eine struct tm"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
msgid "B<#include E<lt>time.hE<gt>>"
msgstr "B<#include E<lt>time.hE<gt>>"

