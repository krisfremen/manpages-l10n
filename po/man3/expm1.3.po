#, fuzzy, no-wrap
msgid "B<float expm1f(float >I<x>B<);>\n"
msgstr "B<double expm1 (double >I<x>B<);>\n"

#. type: Plain text
#: ../english/manpages-dev/man3/expm1.3:40
#, fuzzy, no-wrap
msgid "B<long double expm1l(long double >I<x>B<);>\n"
msgstr "B<double expm1 (double >I<x>B<);>\n"

#. type: Plain text
#: ../english/manpages-dev/man3/expm1.3:43
msgid "Link with I<-lm>."
msgstr ""

#. type: Plain text
#: ../english/manpages-dev/man3/expm1.3:47
msgid ""
"Feature Test Macro Requirements for glibc (see B<feature_test_macros>(7)):"
msgstr ""

#. type: Plain text
#: ../english/manpages-dev/man3/expm1.3:53
msgid ""
"B<expm1>(): _BSD_SOURCE || _SVID_SOURCE || _XOPEN_SOURCE\\ E<gt>=\\ 500 || "
