msgid "ON_EXIT"
msgstr "ON_EXIT"

#. type: TH
#, no-wrap
msgid "2014-08-19"
msgstr "19. August 2014"

#. type: TH
#, no-wrap
msgid "GNU"
msgstr "GNU"

#. type: TH
#, no-wrap
