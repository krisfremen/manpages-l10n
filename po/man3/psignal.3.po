msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#, fuzzy, no-wrap
msgid "B<#include E<lt>signal.hE<gt>>\n"
msgstr "B<#include E<lt>signal.hE<gt>>\n"

#. type: Plain text
#, fuzzy, no-wrap
msgid "B<void psignal(int >I<sig>B<, const char *>I<s>B<);>\n"
