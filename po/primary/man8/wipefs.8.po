#. type: TH
#, no-wrap
msgid "WIPEFS"
msgstr "WIPEFS"

#. type: TH
#, no-wrap
msgid "December 2014"
msgstr "Dezember 2014"

#. type: TH
#, no-wrap
msgid "util-linux"
msgstr "util-linux"

#. type: TH
#, no-wrap
