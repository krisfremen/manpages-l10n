#. type: SH
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
msgid "cksum - checksum and count the bytes in a file"
msgstr "cksum - Prüfsumme und Anzahl der Bytes einer Datei ausgeben"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
msgid "B<cksum> [I<\\,FILE\\/>]..."
msgstr "B<cksum> [I<\\,DATEI\\/>]…"

#. type: Plain text
msgid "B<cksum> [I<\\,OPTION\\/>]"
msgstr "B<cksum> [I<\\,OPTION\\/>]"

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
msgid "Print CRC checksum and byte counts of each FILE."
msgstr "CRC-Prüfsumme und Anzahl der Bytes für jede DATEI ausgeben."

#. type: TP
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
msgid "display this help and exit"
msgstr "Diese Hilfe anzeigen und beenden"

#. type: TP
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
msgid "output version information and exit"
msgstr "Versionsinformation anzeigen und beenden"

