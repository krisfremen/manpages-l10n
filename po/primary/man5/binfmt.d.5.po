#, no-wrap
msgid "EXAMPLE"
msgstr "BEISPIEL"

#. type: Plain text
msgid "B<Example\\ \\&1.\\ \\&/etc/binfmt\\&.d/wine\\&.conf example:>"
msgstr ""

