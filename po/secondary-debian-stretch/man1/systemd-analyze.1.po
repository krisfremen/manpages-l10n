msgstr ""

#. type: Plain text
msgid "B<systemd-analyze> [OPTIONS...] set-log-level I<LEVEL>"
msgstr ""

#. type: Plain text
msgid "B<systemd-analyze> [OPTIONS...] set-log-target I<TARGET>"
msgstr ""

#. type: Plain text
msgid "B<systemd-analyze> [OPTIONS...] get-log-level"
msgstr ""

#. type: Plain text
msgid "B<systemd-analyze> [OPTIONS...] get-log-target"
msgstr ""

#. type: Plain text
msgid "B<systemd-analyze> [OPTIONS...] syscall-filter [I<SET>\\&...]"
msgstr ""

#. type: Plain text
