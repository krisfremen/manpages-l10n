"    do_start_cleanup # no-op\n"
msgstr ""

#. type: Plain text
#: debian-unstable
#, no-wrap
msgid ""
"/etc/init.d/script stop\n"
"  do_stop\n"
"    do_stop_prepare # no-op\n"
"    do_stop_cmd     # start\\-stop\\-daemon is called here\n"
"    do_stop_cleanup # no-op\n"
msgstr ""

#. type: Plain text
#: debian-unstable
#, no-wrap
msgid ""
"/etc/init.d/script status\n"
"  do_status\n"
msgstr ""

#. type: Plain text
#: debian-unstable
#, no-wrap
msgid ""
"/etc/init.d/script reload\n"
"  do_reload\n"
"    do_usage\n"
"    exit 3\n"
msgstr ""

#. type: Plain text
#: debian-unstable
#, no-wrap
msgid ""
"/etc/init.d/script force\\-reload\n"
"  do_force_reload\n"
"    do_reload   # if overridden\n"
"    do_restart\n"
"      do_restart_prepare\n"
"      do_stop_cmd\n"
"      do_start_cmd\n"
"      do_restart_cleanup\n"
msgstr ""

#. type: Plain text
#: debian-unstable
#, no-wrap
msgid ""
"/etc/init.d/script restart\n"
"  do_force_restart\n"
