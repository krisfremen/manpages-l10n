"    ret = ioctl(fd, FAT_IOCTL_GET_ATTRIBUTES, &attr);\n"
"    if (ret == -1) {\n"
"        perror(\"ioctl\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
msgstr ""
"    if (chown(argv[2], uid, -1) == -1) {\n"
"        perror(\"chown\");\n"
"        exit(EXIT_FAILURE);\n"
