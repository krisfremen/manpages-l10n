#. type: TP
#: debian-buster
#, no-wrap
msgid "B<IPC_PRIVATE>"
msgstr "B<IPC_PRIVATE>"

#. type: Plain text
#: debian-buster
msgid "Private key."
msgstr "Clé privée."

#. type: TP
#: debian-buster
#, no-wrap
msgid "B<IPC_RMID>"
msgstr "B<IPC_RMID>"

#. type: Plain text
