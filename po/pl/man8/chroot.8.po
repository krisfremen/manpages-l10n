#. type: TH
#, no-wrap
msgid "CHROOT"
msgstr "CHROOT"

#. type: TH
#, no-wrap
msgid "January 2016"
msgstr "styczeń 2016"

#. type: TH
#, no-wrap
msgid "GNU coreutils 8.25"
msgstr "GNU coreutils 8.25"

#. type: TH
#, no-wrap
msgid "User Commands"
msgstr "Polecenia użytkownika"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "NAZWA"

#. type: Plain text
msgid "chroot - run command or interactive shell with special root directory"
msgstr ""
"chroot - uruchamia polecenie lub powłokę interaktywną ze specjalnym "
"katalogiem głównym"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "SKŁADNIA"

#. type: Plain text
msgid ""
"B<chroot> [I<\\,OPTION\\/>] I<\\,NEWROOT \\/>[I<\\,COMMAND \\/>[I<\\,ARG\\/"
">]...]"
msgstr ""
"B<chroot> [I<OPCJA>] I<NOWY-KATALOG-ROOT >[I<POLECENIE >[I<ARGUMENTY>]...]"

#. type: Plain text
msgid "B<chroot> I<\\,OPTION\\/>"
msgstr "B<chroot> I<OPCJA>"

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "OPIS"

#. type: Plain text
msgid "Run COMMAND with root directory set to NEWROOT."
msgstr ""
"Uruchamia I<POLECENIE> w katalogu głównym B</> ustawionym na I<NOWY-KATALOG-"
"ROOT>."

#. type: TP
#, no-wrap
msgid "B<--groups>=I<\\,G_LIST\\/>"
msgstr "B<--groups>=I<LISTA-GRUP>"

#. type: Plain text
msgid "specify supplementary groups as g1,g2,..,gN"
msgstr "dodatkowe grupy w postaci I<grupa1>B<,>I<grupa2>B<,>..B<,>I<grupaN>"

#. type: TP
#, no-wrap
msgid "B<--userspec>=I<\\,USER\\/>:GROUP"
msgstr "B<--userspec>=I<UŻYTKOWNIK>B<:>I<GRUPA>"

#. type: Plain text
msgid "specify user and group (ID or name) to use"
msgstr ""
"użyty zostanie podany I<UŻYTKOWNIK> i I<GRUPA> (podany jako identyfikator "
"liczbowy lub nazwa)"

#. type: TP
#, no-wrap
msgid "B<--skip-chdir>"
msgstr "B<--skip-chdir>"

#. type: Plain text
msgid "do not change working directory to '/'"
msgstr "nie zmienia katalogu roboczego na \"/\""

#. type: TP
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
msgid "display this help and exit"
msgstr "wyświetla ten tekst i kończy pracę"

#. type: TP
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
msgid "output version information and exit"
msgstr "wyświetla informacje o wersji i kończy działanie"

#. type: Plain text
msgid ""
"If no command is given, run '${SHELL} B<-i>' (default: '/bin/sh B<-i>')."
msgstr ""
"Jeśli nie podano żadnego polecenia, wykonywane jest B<${SHELL} -i> "
"(domyślnie: /bin/sh -i)"

#. type: SH
#, no-wrap
msgid "AUTHOR"
msgstr "AUTOR"

#. type: Plain text
msgid "Written by Roland McGrath."
msgstr "Napisane przez Rolanda McGratha."

#. type: SH
#, no-wrap
msgid "REPORTING BUGS"
msgstr "ZGŁASZANIE BŁĘDÓW"

#. type: Plain text
msgid ""
"GNU coreutils online help: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr ""
"Strona internetowa z pomocą GNU coreutils: E<lt>http://www.gnu.org/software/"
"coreutils/E<gt>"

#. type: Plain text
msgid ""
"Report chroot translation bugs to E<lt>http://translationproject.org/team/"
"E<gt>"
msgstr ""
"Zgłoszenia błędów w tłumaczeniu chroot proszę wysyłać na adres E<lt>http://"
"translationproject.org/team/pl.htmlE<gt>"

#. type: SH
#, no-wrap
msgid "COPYRIGHT"
msgstr "PRAWA AUTORSKIE"

#. type: Plain text
msgid ""
"Copyright \\(co 2016 Free Software Foundation, Inc.  License GPLv3+: GNU GPL "
"version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."
msgstr ""
