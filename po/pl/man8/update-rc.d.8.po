msgid "UPDATE-RC.D"
msgstr "UPDATE-RC.D"

#. type: TH
#, no-wrap
msgid "14 November 2005"
msgstr "14 listopada 2005"

#. type: TH
#, no-wrap
msgid "Debian Project"
msgstr "Projekt Debiana"

#. type: TH
