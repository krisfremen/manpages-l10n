#. type: TH
#, no-wrap
msgid "CAT"
msgstr "CAT"

#. type: TH
#, no-wrap
msgid "January 2016"
msgstr "styczeń 2016"

#. type: TH
#, no-wrap
msgid "GNU coreutils 8.25"
msgstr "GNU coreutils 8.25"

#. type: TH
#, no-wrap
msgid "User Commands"
msgstr "Polecenia użytkownika"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "NAZWA"

#. type: Plain text
msgid "cat - concatenate files and print on the standard output"
msgstr "cat - łączy pliki i wypisuje je na standardowe wyjście"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "SKŁADNIA"

#. type: Plain text
msgid "B<cat> [I<\\,OPTION\\/>]... [I<\\,FILE\\/>]..."
msgstr "B<cat> [I<OPCJA>]... [I<PLIK>]..."

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "OPIS"

#. type: Plain text
msgid "Concatenate FILE(s) to standard output."
msgstr "Łączy I<PLIK(I)> na standardowe wyjście."

#. type: Plain text
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""
"Jeśli nie podano I<PLIKU> lub jako I<PLIK> podano B<->, czyta standardowe "
"wejście."

#. type: TP
#, no-wrap
msgid "B<-A>, B<--show-all>"
msgstr "B<-A>, B<--show-all>"

#. type: Plain text
msgid "equivalent to B<-vET>"
msgstr "równoważne B<-vET>"

#. type: TP
#, no-wrap
msgid "B<-b>, B<--number-nonblank>"
msgstr "B<-b>, B<--number-nonblank>"

#. type: Plain text
msgid "number nonempty output lines, overrides B<-n>"
msgstr "numeruje niepuste linie wyjścia, nadpisując B<-n>"

#. type: TP
#, no-wrap
msgid "B<-e>"
msgstr "B<-e>"

#. type: Plain text
msgid "equivalent to B<-vE>"
msgstr "równoważne B<-vE>"

#. type: TP
#, no-wrap
msgid "B<-E>, B<--show-ends>"
msgstr "B<-E>, B<--show-ends>"

#. type: Plain text
msgid "display $ at end of each line"
msgstr "wyświetla $ na końcu każdego wiersza"

#. type: TP
#, no-wrap
msgid "B<-n>, B<--number>"
msgstr "B<-n>, B<--number>"

#. type: Plain text
msgid "number all output lines"
msgstr "numeruje wszystkie wiersze wyjścia"

#. type: TP
#, no-wrap
msgid "B<-s>, B<--squeeze-blank>"
msgstr "B<-s>, B<--squeeze-blank>"

#. type: Plain text
msgid "suppress repeated empty output lines"
msgstr "zastępuje powielone puste wiersze jednym"

#. type: TP
#, no-wrap
msgid "B<-t>"
msgstr "B<-t>"

#. type: Plain text
msgid "equivalent to B<-vT>"
msgstr "równoważne B<-vT>"

#. type: TP
#, no-wrap
msgid "B<-T>, B<--show-tabs>"
msgstr "B<-T>, B<--show-tabs>"

#. type: Plain text
msgid "display TAB characters as ^I"
msgstr "wyświetla znaki tabulacji jako ^I"

#. type: TP
#, no-wrap
msgid "B<-u>"
msgstr "B<-u>"

#. type: Plain text
msgid "(ignored)"
msgstr "(ignorowana)"

#. type: TP
#, no-wrap
msgid "B<-v>, B<--show-nonprinting>"
msgstr "B<-v>, B<--show-nonprinting>"

#. type: Plain text
msgid "use ^ and M- notation, except for LFD and TAB"
msgstr "używa notacji ^ i M-, z wyjątkiem LFD i TAB"

#. type: TP
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
msgid "display this help and exit"
msgstr "wyświetla ten tekst i kończy pracę"

#. type: TP
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
msgid "output version information and exit"
msgstr "wyświetla informacje o wersji i kończy działanie"

#. type: SH
#, no-wrap
msgid "EXAMPLES"
msgstr "PRZYKŁADY"

#. type: TP
#, no-wrap
msgid "cat f - g"
msgstr "cat f - g"

#. type: Plain text
msgid "Output f's contents, then standard input, then g's contents."
msgstr ""
"Wyświetla zawartość: pliku \"f\", następnie standardowego wyjścia, na końcu "
"pliku \"g\"."

#. type: TP
#, no-wrap
msgid "cat"
msgstr "cat"

#. type: Plain text
msgid "Copy standard input to standard output."
msgstr "Kopiuje standardowe wejście na standardowe wyjście."

#. type: SH
#, no-wrap
msgid "AUTHOR"
msgstr "AUTOR"

#. type: Plain text
msgid "Written by Torbjorn Granlund and Richard M. Stallman."
msgstr "Napisane przez Torbjorna Granlunda i Richarda M. Stallmana."

#. type: SH
#, no-wrap
msgid "REPORTING BUGS"
msgstr "ZGŁASZANIE BŁĘDÓW"

#. type: Plain text
msgid ""
"GNU coreutils online help: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr ""
"Strona internetowa z pomocą GNU coreutils: E<lt>http://www.gnu.org/software/"
"coreutils/E<gt>"

#. type: Plain text
msgid ""
"Report cat translation bugs to E<lt>http://translationproject.org/team/E<gt>"
msgstr ""
"Zgłoszenia błędów w tłumaczeniu cat proszę wysyłać na adres E<lt>http://"
"translationproject.org/team/pl.htmlE<gt>"

#. type: SH
#, no-wrap
msgid "COPYRIGHT"
msgstr "PRAWA AUTORSKIE"

#. type: Plain text
msgid ""
"Copyright \\(co 2016 Free Software Foundation, Inc.  License GPLv3+: GNU GPL "
"version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."
msgstr ""
"Copyright \\(co 2016 Free Software Foundation, Inc. Licencja GPLv3+: GNU GPL "
"w wersji 3 lub późniejszej E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."

#. type: Plain text
msgid ""
"This is free software: you are free to change and redistribute it.  There is "
"NO WARRANTY, to the extent permitted by law."
msgstr ""
"Jest to wolne oprogramowanie: można je zmieniać i rozpowszechniać. Nie ma "
"ŻADNEJ GWARANCJI, w granicach określonych przez prawo."

#. type: SH
#, no-wrap
msgid "SEE ALSO"
msgstr "ZOBACZ TAKŻE"

#. type: Plain text
msgid "B<tac>(1)"
msgstr "B<tac>(1)"

#. type: Plain text
msgid ""
"Full documentation at: E<lt>http://www.gnu.org/software/coreutils/catE<gt>"
msgstr ""
"Pełna dokumentacja na stronie: E<lt>http://www.gnu.org/software/coreutils/"
"catE<gt>"

#. type: Plain text
msgid "or available locally via: info \\(aq(coreutils) cat invocation\\(aq"
msgstr "lub lokalnie, za pomocą B<info \\(aq(coreutils) cat invocation\\(aq>"

