#. type: SH
#, no-wrap
msgid "SEE ALSO"
msgstr "ZOBACZ TAKŻE"

#. type: Plain text
msgid "printf(3)"
msgstr "B<printf>(3)"

#. type: Plain text
msgid ""
"Full documentation at: E<lt>http://www.gnu.org/software/coreutils/printfE<gt>"
msgstr ""
