#. type: TH
#, no-wrap
msgid "UNAME"
msgstr "UNAME"

#. type: TH
#, no-wrap
msgid "January 2016"
msgstr "styczeń 2016"

#. type: TH
#, no-wrap
msgid "GNU coreutils 8.25"
msgstr "GNU coreutils 8.25"

#. type: TH
#, no-wrap
msgid "User Commands"
msgstr "Polecenia użytkownika"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "NAZWA"

#. type: Plain text
msgid "uname - print system information"
msgstr "uname - wyświetla informacje o systemie"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "SKŁADNIA"

#. type: Plain text
msgid "B<uname> [I<\\,OPTION\\/>]..."
msgstr "B<uname> [I<OPCJA>]..."

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "OPIS"

#. type: Plain text
msgid "Print certain system information.  With no OPTION, same as B<-s>."
msgstr ""
"Wyświetla informacje o systemie. Bez I<OPCJI> działa jak z opcją B<-s>."

#. type: TP
#, no-wrap
msgid "B<-a>, B<--all>"
msgstr "B<-a>, B<--all>"

#. type: Plain text
msgid ""
"print all information, in the following order, except omit B<-p> and B<-i> "
"if unknown:"
msgstr ""
"wyświetla wszystkie informacje w poniższej kolejności, poza tym, że wynik "
"opcji B<-p> i B<-i> nie jest wyświetlany, jeśli jest nieznany"

#. type: TP
#, no-wrap
msgid "B<-s>, B<--kernel-name>"
msgstr "B<-s>, B<--kernel-name>"

#. type: Plain text
msgid "print the kernel name"
msgstr "wyświetla nazwę jądra"

#. type: TP
#, no-wrap
msgid "B<-n>, B<--nodename>"
msgstr "B<-n>, B<--nodename>"

#. type: Plain text
msgid "print the network node hostname"
msgstr "wyświetla sieciową nazwę systemu"

#. type: TP
#, no-wrap
msgid "B<-r>, B<--kernel-release>"
msgstr "B<-r>, B<--kernel-release>"

#. type: Plain text
msgid "print the kernel release"
msgstr "wyświetla numer wydania jądra"

#. type: TP
#, no-wrap
msgid "B<-v>, B<--kernel-version>"
msgstr "B<-v>, B<--kernel-version>"

#. type: Plain text
msgid "print the kernel version"
msgstr "wyświetla wersję jądra"

#. type: TP
#, no-wrap
msgid "B<-m>, B<--machine>"
msgstr "B<-m>, B<--machine>"

#. type: Plain text
msgid "print the machine hardware name"
msgstr "wyświetla nazwę sprzętu (architekturę) komputera"

#. type: TP
#, no-wrap
msgid "B<-p>, B<--processor>"
msgstr "B<-p>, B<--processor>"

#. type: Plain text
msgid "print the processor type (non-portable)"
msgstr "wyświetla typ procesora (nieprzenośne)"

#. type: TP
#, no-wrap
msgid "B<-i>, B<--hardware-platform>"
msgstr "B<-i>, B<--hardware-platform>"

#. type: Plain text
msgid "print the hardware platform (non-portable)"
msgstr "wyświetla platformę sprzętową (nieprzenośne)"

#. type: TP
#, no-wrap
msgid "B<-o>, B<--operating-system>"
msgstr "B<-o>, B<--operating-system>"

#. type: Plain text
msgid "print the operating system"
msgstr "wyświetla system operacyjny"

#. type: TP
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
msgid "display this help and exit"
msgstr "wyświetla ten tekst i kończy pracę"

#. type: TP
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
msgid "output version information and exit"
msgstr "wyświetla informacje o wersji i kończy działanie"

#. type: SH
#, no-wrap
msgid "AUTHOR"
msgstr "AUTOR"

#. type: Plain text
msgid "Written by David MacKenzie."
msgstr "Napisane przez Davida MacKenzie."

#. type: SH
#, no-wrap
msgid "REPORTING BUGS"
msgstr "ZGŁASZANIE BŁĘDÓW"

#. type: Plain text
msgid ""
"GNU coreutils online help: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr ""
"Strona internetowa z pomocą GNU coreutils: E<lt>http://www.gnu.org/software/"
"coreutils/E<gt>"

#. type: Plain text
msgid ""
"Report uname translation bugs to E<lt>http://translationproject.org/team/"
"E<gt>"
msgstr ""
"Zgłoszenia błędów w tłumaczeniu uname proszę wysyłać na adres E<lt>http://"
"translationproject.org/team/pl.htmlE<gt>"

#. type: SH
#, no-wrap
msgid "COPYRIGHT"
msgstr "PRAWA AUTORSKIE"

#. type: Plain text
msgid ""
"Copyright \\(co 2016 Free Software Foundation, Inc.  License GPLv3+: GNU GPL "
"version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."
msgstr ""
"Copyright \\(co 2016 Free Software Foundation, Inc. Licencja GPLv3+: GNU GPL "
"w wersji 3 lub późniejszej E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."

#. type: Plain text
msgid ""
"This is free software: you are free to change and redistribute it.  There is "
"NO WARRANTY, to the extent permitted by law."
msgstr ""
"Jest to wolne oprogramowanie: można je zmieniać i rozpowszechniać. Nie ma "
"ŻADNEJ GWARANCJI, w granicach określonych przez prawo."

#. type: SH
#, no-wrap
msgid "SEE ALSO"
msgstr "ZOBACZ TAKŻE"

#. type: Plain text
msgid "arch(1), uname(2)"
msgstr "B<arch>(1), B<uname>(2)"

#. type: Plain text
msgid ""
"Full documentation at: E<lt>http://www.gnu.org/software/coreutils/unameE<gt>"
msgstr ""
"Pełna dokumentacja na stronie: E<lt>http://www.gnu.org/software/coreutils/"
"unameE<gt>"

#. type: Plain text
msgid "or available locally via: info \\(aq(coreutils) uname invocation\\(aq"
msgstr "lub lokalnie, za pomocą B<info \\(aq(coreutils) uname invocation\\(aq>"

