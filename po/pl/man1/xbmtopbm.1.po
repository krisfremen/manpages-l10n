#. type: IX
#, no-wrap
msgid "xbmtopbm"
msgstr "xbmtopbm"

#. type: TH
#, no-wrap
msgid "31 August 1988"
msgstr "31 sierpnia 1988"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "NAZWA"

#
#. type: Plain text
msgid "xbmtopbm - convert an X11 or X10 bitmap into a portable bitmap"
msgstr "xbmtopbm - tworzy przenośną bitmapę (.pbm) z bitmapy typu X11 lub X10"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "SKŁADNIA"

#
#. type: Plain text
msgid "B<xbmtopbm> [I<bitmapfile>]"
msgstr "B<xbmtopbm> [I<plik_bitmapa>]"

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "OPIS"

#
#. type: Plain text
msgid ""
"Reads an X11 or X10 bitmap as input.  Produces a portable bitmap as output."
msgstr ""
"Odczytuje bitmapę typu X11 lub X10. Następnie zapisuje na standardowe "
"wyjście przenośną bitmapę (.pbm)."

#. type: IX
#, no-wrap
msgid "X bitmap"
msgstr "X bitmap"

#. type: IX
#, no-wrap
msgid "X window system"
msgstr "X window system"

