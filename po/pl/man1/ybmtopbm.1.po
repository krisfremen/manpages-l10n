#. type: IX
#, no-wrap
msgid "ybmtopbm"
msgstr "ybmtopbm"

#. type: TH
#, no-wrap
msgid "06 March 1990"
msgstr "6 marca 1990"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "NAZWA"

#
#. type: Plain text
msgid "ybmtopbm - convert a Bennet Yee \"face\" file into a portable bitmap"
msgstr "ybmtopbm - tworzy przenośną bitmapę (pbm) z pliku typu \"face\""

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "SKŁADNIA"

#
#. type: Plain text
msgid "B<ybmtopbm> [I<facefile>]"
msgstr "B<ybmtopbm> [I<plik_face>]"

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "OPIS"

#
#. type: Plain text
msgid ""
"Reads a file acceptable to the I<face> and I<xbm> programs by Bennet Yee (bsy"
"+@cs.cmu.edu)."
msgstr ""
"Odczytuje plik w formacie rozpoznawalnym przez programy I<face> i I<xbm> "
"napisane przez Benneta Yee (bsy+@cs.cmu.edu)."

#. type: IX
#, no-wrap
msgid "face"
msgstr "face"

#
#. type: Plain text
msgid "Writes a portable bitmap as output."
msgstr ""
"Następnie zapisuje na standardowe wyjście przenośną bitmapę (plik .pbm)."

#. type: SH
#, no-wrap
msgid "SEE ALSO"
msgstr "ZOBACZ TAKŻE"

#
#. type: Plain text
msgid "pbmtoybm(1), pbm(5), face(1), face(5), xbm(1)"
msgstr "pbmtoybm(1), pbm(5), face(1), face(5), xbm(1)"

