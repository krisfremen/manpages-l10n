#. type: TH
#, no-wrap
msgid "PTX"
msgstr "PTX"

#. type: TH
#, no-wrap
msgid "January 2016"
msgstr "styczeń 2016"

#. type: TH
#, no-wrap
msgid "GNU coreutils 8.25"
msgstr "GNU coreutils 8.25"

#. type: TH
#, no-wrap
msgid "User Commands"
msgstr "Polecenia użytkownika"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "NAZWA"

#. type: Plain text
msgid "ptx - produce a permuted index of file contents"
msgstr "ptx - tworzy indeks permutacyjny z zawartości pliku"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "SKŁADNIA"

#. type: Plain text
msgid "B<ptx> [I<\\,OPTION\\/>]... [I<\\,INPUT\\/>]...  I<\\,(without -G)\\/>"
msgstr "B<ptx> [I<OPCJA>]... [I<WEJŚCIE>]...\t(bez -G)"

#. type: Plain text
msgid ""
"B<ptx> I<\\,-G \\/>[I<\\,OPTION\\/>]... [I<\\,INPUT \\/>[I<\\,OUTPUT\\/>]]"
msgstr "B<ptx> B<-G >[I<OPCJA>]... [I<WEJŚCIE >[I<WYJŚCIE>]]"

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "OPIS"

#. type: Plain text
msgid ""
"Output a permuted index, including context, of the words in the input files."
msgstr ""
"Wypisuje indeks permutacyjny, obejmujący kontekst, ze słów z plików "
"wejściowych."

#. type: Plain text
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""
"Jeśli nie podano I<PLIKU> lub jako I<PLIK> podano B<->, czyta standardowe "
"wejście."

#. type: Plain text
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""
"Argumenty, które są obowiązkowe dla długich opcji, są również obowiązkowe "
"dla krótkich."

#. type: TP
#, no-wrap
msgid "B<-A>, B<--auto-reference>"
msgstr "B<-A>, B<--auto-reference>"

#. type: Plain text
msgid "output automatically generated references"
msgstr "wyświetla automatycznie utworzone przypisy"

#. type: TP
#, no-wrap
msgid "B<-G>, B<--traditional>"
msgstr "B<-G>, B<--traditional>"

#. type: Plain text
msgid "behave more like System V 'ptx'"
msgstr "zachowuje się zgodnie z wersją B<ptx> z System V"

#. type: TP
#, no-wrap
msgid "B<-F>, B<--flag-truncation>=I<\\,STRING\\/>"
msgstr "B<-F>, B<--flag-truncation>=I<ŁAŃCUCH>"

#. type: Plain text
msgid "use STRING for flagging line truncations.  The default is '/'"
msgstr "używa I<ŁAŃCUCHA> do oznaczania wyciętych wierszy. Domyślnie \"/\"."

#. type: TP
#, no-wrap
msgid "B<-M>, B<--macro-name>=I<\\,STRING\\/>"
msgstr "B<-M>, B<--macro-name>=I<ŁAŃCUCH>"

#. type: Plain text
msgid "macro name to use instead of 'xx'"
msgstr "nazwa makra używana zamiast B<xx>"

#. type: TP
#, no-wrap
msgid "B<-O>, B<--format>=I<\\,roff\\/>"
msgstr "B<-O>, B<--format=roff>"

#. type: Plain text
msgid "generate output as roff directives"
msgstr "tworzy wyjście w postaci dyrektyw roff"

#. type: TP
#, no-wrap
msgid "B<-R>, B<--right-side-refs>"
msgstr "B<-R>, B<--right-side-refs>"

#. type: Plain text
msgid "put references at right, not counted in B<-w>"
msgstr "umieszcza przypisy po prawej stronie, nie liczone w opcji B<-w>"

#. type: TP
#, no-wrap
msgid "B<-S>, B<--sentence-regexp>=I<\\,REGEXP\\/>"
msgstr "B<-S>, B<--sentence-regexp>=I<WYRAŻENIE-REGULARNE>"

#. type: Plain text
msgid "for end of lines or end of sentences"
msgstr "użycie podanego wyrażenia do określenia końców wierszy lub zdań"

#. type: TP
#, no-wrap
msgid "B<-T>, B<--format>=I<\\,tex\\/>"
msgstr "B<-T>, B<--format=tex>"

#. type: Plain text
msgid "generate output as TeX directives"
msgstr "tworzy wyjście w postaci dyrektyw TeX-a"

#. type: TP
#, no-wrap
msgid "B<-W>, B<--word-regexp>=I<\\,REGEXP\\/>"
msgstr "B<-W>, B<--word-regexp>=I<WYRAŻENIE-REGULARNE>"

#. type: Plain text
msgid "use REGEXP to match each keyword"
msgstr "używa I<WYRAŻENIA-REGULARNEGO> w celu dopasowania słów kluczowych"

#. type: TP
#, no-wrap
msgid "B<-b>, B<--break-file>=I<\\,FILE\\/>"
msgstr "B<-b>, B<--break-file>=I<PLIK>"

#. type: Plain text
msgid "word break characters in this FILE"
msgstr "znaki rozdzielające słowa umieszczone są w I<PLIKU>"

#. type: TP
#, no-wrap
msgid "B<-f>, B<--ignore-case>"
msgstr "B<-f>, B<--ignore-case>"

#. type: Plain text
msgid "fold lower case to upper case for sorting"
msgstr "w sortowaniu traktuje małe litery jak wielkie"

#. type: TP
#, no-wrap
msgid "B<-g>, B<--gap-size>=I<\\,NUMBER\\/>"
msgstr "B<-g>, B<--gap-size>=I<LICZBA>"

#. type: Plain text
msgid "gap size in columns between output fields"
msgstr "określa rozmiar odstępów pomiędzy polami w kolumnach"

#. type: TP
#, no-wrap
msgid "B<-i>, B<--ignore-file>=I<\\,FILE\\/>"
msgstr "B<-i>, B<--ignore-file>=I<PLIK>"

#. type: Plain text
msgid "read ignore word list from FILE"
msgstr "odczytuje listę ignorowanych słów z I<PLIKU>"

#. type: TP
#, no-wrap
msgid "B<-o>, B<--only-file>=I<\\,FILE\\/>"
msgstr "B<-o>, B<--only-file>=I<PLIK>"

#. type: Plain text
msgid "read only word list from this FILE"
msgstr "odczytuje tylko słowa z listy słów z I<PLIKU>"

#. type: TP
#, no-wrap
msgid "B<-r>, B<--references>"
msgstr "B<-r>, B<--references>"

#. type: Plain text
msgid "first field of each line is a reference"
msgstr "pierwsze pole w każdym wierszu jest przypisem"

#. type: Plain text
msgid "B<-t>, B<--typeset-mode> - not implemented -"
msgstr "B<-t>, B<--typeset-mode> (nie zaimplementowane)"

#. type: TP
#, no-wrap
msgid "B<-w>, B<--width>=I<\\,NUMBER\\/>"
msgstr "B<-w>, B<--width>=I<LICZBA>"

#. type: Plain text
msgid "output width in columns, reference excluded"
msgstr "szerokość wyjściowa kolumn, wyłączając przypisy"

#. type: TP
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
msgid "display this help and exit"
msgstr "wyświetla ten tekst i kończy pracę"

#. type: TP
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
msgid "output version information and exit"
msgstr "wyświetla informacje o wersji i kończy działanie"

#. type: SH
#, no-wrap
msgid "AUTHOR"
msgstr "AUTOR"

#. type: Plain text
msgid "Written by F. Pinard."
msgstr "Napisane przez F. Pinarda."

#. type: SH
#, no-wrap
msgid "REPORTING BUGS"
msgstr "ZGŁASZANIE BŁĘDÓW"

