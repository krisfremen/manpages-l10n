#. type: TH
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "ECHO"
msgstr "ECHO"

#. type: TH
#: archlinux
#, fuzzy, no-wrap
#| msgid "September 2014"
msgid "November 2019"
msgstr "wrzesień 2014"

#. type: TH
#: archlinux mageia-cauldron
#, fuzzy, no-wrap
#| msgid "GNU coreutils 8.21"
msgid "GNU coreutils 8.31"
msgstr "GNU coreutils 8.21"

#. type: TH
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "User Commands"
msgstr "Polecenia użytkownika"

#. type: SH
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "NAME"
msgstr "NAZWA"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "echo - display a line of text"
msgstr "echo - wyświetla wiersz tekstu"

#. type: SH
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "SYNOPSIS"
msgstr "SKŁADNIA"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "B<echo> [I<\\,SHORT-OPTION\\/>]... [I<\\,STRING\\/>]..."
msgstr "B<echo> [I<KRÓTKA-OPCJA>]... [I<NAPIS>]..."

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "B<echo> I<\\,LONG-OPTION\\/>"
msgstr "B<echo> I<DŁUGA-OPCJA>"

#. type: SH
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "DESCRIPTION"
msgstr "OPIS"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "Echo the STRING(s) to standard output."
msgstr "Wyświetla I<NAPIS(Y)> na standardowe wyjście."

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "B<-n>"
msgstr "B<-n>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "do not output the trailing newline"
msgstr "nie wypisuje kończącego znaku nowego wiersza"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "B<-e>"
msgstr "B<-e>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "enable interpretation of backslash escapes"
msgstr "włącza interpretowania sekwencji specjalnych z ukośnikiem"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "B<-E>"
msgstr "B<-E>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "disable interpretation of backslash escapes (default)"
msgstr "wyłącza interpretowanie sekwencji specjalnych z ukośnikiem (domyślnie)"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "display this help and exit"
msgstr "wyświetla ten tekst i kończy pracę"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "output version information and exit"
msgstr "wyświetla informacje o wersji i kończy działanie"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "If B<-e> is in effect, the following sequences are recognized:"
msgstr ""
"Jeśli użyto opcji B<-e>, to rozpoznawane są następujące sekwencje specjalne:"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "\\e\\e"
msgstr "B<\\e\\e>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "backslash"
msgstr "odwrotny ukośnik"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "\\ea"
msgstr "B<\\ea>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "alert (BEL)"
msgstr "dzwonek, alarm (BEL)"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "\\eb"
msgstr "B<\\eb>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "backspace"
msgstr "backspace"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "\\ec"
msgstr "B<\\ec>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "produce no further output"
msgstr "kończy wypisywanie wyniku"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "\\ee"
msgstr "B<\\ee>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "escape"
msgstr "escape"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "\\ef"
msgstr "B<\\ef>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "form feed"
msgstr "wysuw strony"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "\\en"
msgstr "B<\\en>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "new line"
msgstr "znak nowego wiersza"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "\\er"
msgstr "B<\\er>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "carriage return"
msgstr "powrót karetki"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "\\et"
msgstr "B<\\et>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "horizontal tab"
msgstr "tabulacja pozioma"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "\\ev"
msgstr "B<\\ev>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "vertical tab"
msgstr "tabulacja pionowa"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "\\e0NNN"
msgstr "B<\\e0>I<NNN>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "byte with octal value NNN (1 to 3 digits)"
msgstr "bajt o wartości ósemkowej I<NNN> (1 do 3 cyfr)"

#. type: TP
#: archlinux debian-buster debian-unstable mageia-cauldron
#, no-wrap
msgid "\\exHH"
msgstr "B<\\ex>I<HH>"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid "byte with hexadecimal value HH (1 to 2 digits)"
msgstr "bajt o wartości szesnastkowej I<HH> (1 do 2 cyfr)"

#. type: Plain text
#: archlinux debian-buster debian-unstable mageia-cauldron
msgid ""
"NOTE: your shell may have its own version of echo, which usually supersedes "
"the version described here.  Please refer to your shell's documentation for "
"details about the options it supports."
msgstr ""
"B<UWAGA>: aktualnie używana powłoka może posiadać swoją wersję B<echo>, "
"która z reguły przesłania wersję opisaną w niniejszym podręczniku. Proszę "
"zapoznać się z dokumentacją używanej powłoki, aby dowiedzieć się szczegółów "
"o obsługiwanych opcjach. Aby wywołać opisaną tu wersję, można użyć polecenia "
"B<env> tzn. B<env echo> I<...>."

