#. type: TH
#, no-wrap
msgid "ECHO"
msgstr "ECHO"

#. type: TH
#, no-wrap
msgid "January 2016"
msgstr "styczeń 2016"

#. type: TH
#, no-wrap
msgid "GNU coreutils 8.25"
msgstr "GNU coreutils 8.25"

#. type: TH
#, no-wrap
msgid "User Commands"
msgstr "Polecenia użytkownika"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "NAZWA"

#. type: Plain text
msgid "echo - display a line of text"
msgstr "echo - wyświetla wiersz tekstu"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "SKŁADNIA"

#. type: Plain text
msgid "B<echo> [I<\\,SHORT-OPTION\\/>]... [I<\\,STRING\\/>]..."
msgstr "B<echo> [I<KRÓTKA-OPCJA>]... [I<NAPIS>]..."

#. type: Plain text
msgid "B<echo> I<\\,LONG-OPTION\\/>"
msgstr "B<echo> I<DŁUGA-OPCJA>"

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "OPIS"

#. type: Plain text
msgid "Echo the STRING(s) to standard output."
msgstr "Wyświetla I<NAPIS(Y)> na standardowe wyjście."

#. type: TP
#, no-wrap
msgid "B<-n>"
msgstr "B<-n>"

#. type: Plain text
msgid "do not output the trailing newline"
msgstr "nie wypisuje kończącego znaku nowego wiersza"

#. type: TP
#, no-wrap
msgid "B<-e>"
msgstr "B<-e>"

#. type: Plain text
msgid "enable interpretation of backslash escapes"
msgstr "włącza interpretowania sekwencji specjalnych z ukośnikiem"

#. type: TP
#, no-wrap
msgid "B<-E>"
msgstr "B<-E>"

#. type: Plain text
msgid "disable interpretation of backslash escapes (default)"
msgstr "wyłącza interpretowanie sekwencji specjalnych z ukośnikiem (domyślnie)"

#. type: TP
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
msgid "display this help and exit"
msgstr "wyświetla ten tekst i kończy pracę"

#. type: TP
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
msgid "output version information and exit"
msgstr "wyświetla informacje o wersji i kończy działanie"

#. type: Plain text
msgid "If B<-e> is in effect, the following sequences are recognized:"
msgstr ""
"Jeśli użyto opcji B<-e>, to rozpoznawane są następujące sekwencje specjalne:"

#. type: TP
#, no-wrap
msgid "\\e\\e"
msgstr "B<\\e\\e>"

#. type: Plain text
msgid "backslash"
msgstr "odwrotny ukośnik"

#. type: TP
#, no-wrap
msgid "\\ea"
msgstr "B<\\ea>"

#. type: Plain text
msgid "alert (BEL)"
msgstr "dzwonek, alarm (BEL)"

#. type: TP
#, no-wrap
msgid "\\eb"
msgstr "B<\\eb>"

#. type: Plain text
msgid "backspace"
msgstr "backspace"

#. type: TP
#, no-wrap
msgid "\\ec"
msgstr "B<\\ec>"

#. type: Plain text
msgid "produce no further output"
msgstr "kończy wypisywanie wyniku"

#. type: TP
#, no-wrap
msgid "\\ee"
msgstr "B<\\ee>"

#. type: Plain text
msgid "escape"
msgstr "escape"

#. type: TP
#, no-wrap
msgid "\\ef"
msgstr "B<\\ef>"

#. type: Plain text
msgid "form feed"
msgstr "wysuw strony"

#. type: TP
#, no-wrap
msgid "\\en"
msgstr "B<\\en>"

#. type: Plain text
msgid "new line"
msgstr "znak nowego wiersza"

#. type: TP
#, no-wrap
msgid "\\er"
msgstr "B<\\er>"

#. type: Plain text
msgid "carriage return"
msgstr "powrót karetki"

#. type: TP
#, no-wrap
msgid "\\et"
msgstr "B<\\et>"

#. type: Plain text
msgid "horizontal tab"
msgstr "tabulacja pozioma"

#. type: TP
#, no-wrap
msgid "\\ev"
msgstr "B<\\ev>"

#. type: Plain text
msgid "vertical tab"
msgstr "tabulacja pionowa"

#. type: TP
#, no-wrap
msgid "\\e0NNN"
msgstr "B<\\e0>I<NNN>"

#. type: Plain text
msgid "byte with octal value NNN (1 to 3 digits)"
msgstr "bajt o wartości ósemkowej I<NNN> (1 do 3 cyfr)"

#. type: TP
#, no-wrap
msgid "\\exHH"
msgstr "B<\\ex>I<HH>"

#. type: Plain text
msgid "byte with hexadecimal value HH (1 to 2 digits)"
msgstr "bajt o wartości szesnastkowej I<HH> (1 do 2 cyfr)"

#. type: Plain text
msgid ""
"NOTE: your shell may have its own version of echo, which usually supersedes "
"the version described here.  Please refer to your shell's documentation for "
"details about the options it supports."
msgstr ""
"B<UWAGA>: aktualnie używana powłoka może posiadać swoją wersję B<echo>, "
"która z reguły przesłania wersję opisaną w niniejszym podręczniku. Proszę "
"zapoznać się z dokumentacją używanej powłoki, aby dowiedzieć się szczegółów "
"o obsługiwanych opcjach. Aby wywołać opisaną tu wersję, można użyć polecenia "
"B<env> tzn. B<env echo> I<...>."

#. type: SH
#, no-wrap
msgid "AUTHOR"
msgstr "AUTOR"

#. type: Plain text
msgid "Written by Brian Fox and Chet Ramey."
msgstr "Napisane przez Briana Foxa i Cheta Rameya."

#. type: SH
#, no-wrap
msgid "REPORTING BUGS"
msgstr "ZGŁASZANIE BŁĘDÓW"

#. type: Plain text
msgid ""
"GNU coreutils online help: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr ""
"Strona internetowa z pomocą GNU coreutils: E<lt>http://www.gnu.org/software/"
"coreutils/E<gt>"

#. type: Plain text
msgid ""
"Report echo translation bugs to E<lt>http://translationproject.org/team/E<gt>"
msgstr ""
"Zgłoszenia błędów w tłumaczeniu echo proszę wysyłać na adres E<lt>http://"
"translationproject.org/team/pl.htmlE<gt>"

#. type: SH
#, no-wrap
msgid "COPYRIGHT"
msgstr "PRAWA AUTORSKIE"

#. type: Plain text
msgid ""
"Copyright \\(co 2016 Free Software Foundation, Inc.  License GPLv3+: GNU GPL "
"version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."
msgstr ""
