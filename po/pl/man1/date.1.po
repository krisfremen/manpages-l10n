#. type: TH
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "DATE"
msgstr "DATE"

#. type: TH
#: archlinux mageia-cauldron
#, no-wrap
msgid "March 2020"
msgstr "marzec 2020"

#. type: TH
#: archlinux fedora-rawhide mageia-cauldron
#, no-wrap
msgid "GNU coreutils 8.32"
msgstr "GNU coreutils 8.32"

#. type: TH
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "User Commands"
msgstr "Polecenia użytkownika"

#. type: SH
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "NAME"
msgstr "NAZWA"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "date - print or set the system date and time"
msgstr "date - wyświetla lub ustawia datę i czas systemowy"

#. type: SH
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "SYNOPSIS"
msgstr "SKŁADNIA"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "B<date> [I<\\,OPTION\\/>]... [I<\\,+FORMAT\\/>]"
msgstr "B<date> [I<OPCJA>]... [B<+>I<FORMAT>]"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid ""
"B<date> [I<\\,-u|--utc|--universal\\/>] [I<\\,MMDDhhmm\\/>[[I<\\,CC\\/>]I<\\,"
"YY\\/>][I<\\,.ss\\/>]]"
msgstr ""
"B<date> [B<-u>|B<--utc>|B<--universal>] [I<MMDDhhmm>[[I<CC>]I<YY>][I<.ss>]]"

#. type: SH
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "DESCRIPTION"
msgstr "OPIS"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "Display the current time in the given FORMAT, or set the system date."
msgstr ""
"Wyświetla aktualny czas w podanym I<FORMACIE> lub ustawia datę systemową."

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""
"Argumenty, które są obowiązkowe dla długich opcji, są również obowiązkowe "
"dla krótkich."

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<-d>, B<--date>=I<\\,STRING\\/>"
msgstr "B<-d>, B<--date>=I<ŁAŃCUCH>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "display time described by STRING, not 'now'"
msgstr "wyświetla czas podany w I<ŁAŃCUCHU>, nie aktualny"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, fuzzy, no-wrap
#| msgid "B<--help>"
msgid "B<--debug>"
msgstr "B<--help>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "annotate the parsed date, and warn about questionable usage to stderr"
msgstr ""

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<-f>, B<--file>=I<\\,DATEFILE\\/>"
msgstr "B<-f>, B<--file>=I<PLIK-DAT>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "like B<--date>; once for each line of DATEFILE"
msgstr "jak B<--date>; dla każdego wiersza I<PLIKU-DAT>"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<-I[FMT]>, B<--iso-8601>[=I<\\,FMT\\/>]"
msgstr "B<-I[FORMAT]>, B<--iso-8601>[=I<FORMAT>]"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, fuzzy
#| msgid ""
#| "output date/time in ISO 8601 format.  FMT='date' for date only (the "
#| "default), \\&'hours', 'minutes', 'seconds', or 'ns' for date and time to "
#| "the indicated precision.  Example: 2006-08-14T02:34:56-0600"
msgid ""
"output date/time in ISO 8601 format.  FMT='date' for date only (the "
"default), \\&'hours', 'minutes', 'seconds', or 'ns' for date and time to the "
"indicated precision.  Example: 2006-08-14T02:34:56-06:00"
msgstr ""
"wypisuje datę/czas w formacie ISO 8601. I<FORMAT>=B<date> daje samą datę "
"(domyślnie), natomiast B<hours>, B<minutes>, B<seconds> lub B<ns> daje datę "
"i czas o określonej precyzji. Przykład: 2006-08-14T02:34:56-0600"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, fuzzy, no-wrap
#| msgid "B<-R>, B<--rfc-2822>"
msgid "B<-R>, B<--rfc-email>"
msgstr "B<-R>, B<--rfc-2822>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, fuzzy
#| msgid ""
#| "output date and time in RFC 2822 format.  Example: Mon, 14 Aug 2006 "
#| "02:34:56 B<-0600>"
msgid ""
"output date and time in RFC 5322 format.  Example: Mon, 14 Aug 2006 02:34:56 "
"B<-0600>"
msgstr ""
"Wyświetla czas i datę w formacie RFC 2822. Przykład: Mon, 14 Aug 2006 "
"02:34:56 -0600"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<--rfc-3339>=I<\\,FMT\\/>"
msgstr "B<--rfc-3339>=I<FORMAT>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid ""
"output date/time in RFC 3339 format.  FMT='date', 'seconds', or 'ns' for "
"date and time to the indicated precision.  Example: 2006-08-14 02:34:56-06:00"
msgstr ""
"wyświetla czas i datę w formacie RFC 3339. I<FORMAT>=B<date>, B<seconds> lub "
"B<ns> wyświetla czas i datę z żądaną precyzją. Przykład 2006-08-14 "
"02:34:56-06:00"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<-r>, B<--reference>=I<\\,FILE\\/>"
msgstr "B<-r>, B<--reference>=I<PLIK>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "display the last modification time of FILE"
msgstr "wyświetla czas ostatnie modyfikacji I<PLIKU>"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<-s>, B<--set>=I<\\,STRING\\/>"
msgstr "B<-s>, B<--set>=I<ŁAŃCUCH>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "set time described by STRING"
msgstr "ustawia czas na opisany w I<ŁAŃCUCHU>"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<-u>, B<--utc>, B<--universal>"
msgstr "B<-u>, B<--utc>, B<--universal>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "print or set Coordinated Universal Time (UTC)"
msgstr "wyświetla lub ustawia czas uniwersalny (UTC)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "display this help and exit"
msgstr "wyświetla ten tekst i kończy pracę"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "output version information and exit"
msgstr "wyświetla informacje o wersji i kończy działanie"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "FORMAT controls the output.  Interpreted sequences are:"
msgstr "I<FORMAT> określa wyjście. Rozpoznawane sekwencje:"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%%"
msgstr "B<%%>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "a literal %"
msgstr "literalny znak procenta %"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%a"
msgstr "B<%a>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "locale's abbreviated weekday name (e.g., Sun)"
msgstr "lokalny skrót nazwy dnia tygodnia (np. nie)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%A"
msgstr "B<%A>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "locale's full weekday name (e.g., Sunday)"
msgstr "lokalna pełna nazwa dnia tygodnia (np. niedziela)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%b"
msgstr "B<%b>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "locale's abbreviated month name (e.g., Jan)"
msgstr "lokalny skrót nazwy miesiąca (np. sty)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%B"
msgstr "B<%B>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "locale's full month name (e.g., January)"
msgstr "lokalna pełna nazwa miesiąca (np. styczeń)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%c"
msgstr "B<%c>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "locale's date and time (e.g., Thu Mar 3 23:05:25 2005)"
msgstr "lokalna data i czas (np. czw mar 3 23:05:25 2005)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%C"
msgstr "B<%C>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "century; like %Y, except omit last two digits (e.g., 20)"
msgstr ""
"stulecie, jak B<%Y>, tylko z obciętymi ostatnimi dwiema cyframi (np. 20)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%d"
msgstr "B<%d>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "day of month (e.g., 01)"
msgstr "dzień miesiąca (np. 01)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%D"
msgstr "B<%D>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "date; same as %m/%d/%y"
msgstr "data, to samo co B<%m/%d/%y>"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%e"
msgstr "B<%e>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "day of month, space padded; same as %_d"
msgstr "dzień miesiąca uzupełniony spacjami, jak B<%_d>"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%F"
msgstr "B<%F>"

#. type: Plain text
#: archlinux fedora-rawhide mageia-cauldron
#, fuzzy
#| msgid "full date; same as %Y-%m-%d"
msgid "full date; like %+4Y-%m-%d"
msgstr "pełna data; to samo co B<%Y-%m-%d>"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%g"
msgstr "B<%g>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "last two digits of year of ISO week number (see %G)"
msgstr "ostatnie dwie cyfry roku numeru tygodnia ISO (patrz B<%G>)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%G"
msgstr "B<%G>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "year of ISO week number (see %V); normally useful only with %V"
msgstr ""
"rok numeru tygodnia ISO (patrz B<%V>); użyteczny zwykle jedynie z B<%V>"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%h"
msgstr "B<%h>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "same as %b"
msgstr "to samo co B<%b>"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%H"
msgstr "B<%H>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "hour (00..23)"
msgstr "godzina (00..23)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%I"
msgstr "B<%I>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "hour (01..12)"
msgstr "godzina (01..12)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%j"
msgstr "B<%j>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "day of year (001..366)"
msgstr "dzień roku (001..366)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%k"
msgstr "B<%k>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "hour, space padded ( 0..23); same as %_H"
msgstr "godzina, uzupełniona spacją ( 0..23); to samo co B<%_H>"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%l"
msgstr "B<%l>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "hour, space padded ( 1..12); same as %_I"
msgstr "godzina, uzupełniona spacją ( 1..12); to samo co B<%_I>"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%m"
msgstr "B<%m>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "month (01..12)"
msgstr "miesiąc (01..12)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%M"
msgstr "B<%M>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "minute (00..59)"
msgstr "minuta (00..59)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%n"
msgstr "B<%n>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "a newline"
msgstr "znak nowego wiersza"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%N"
msgstr "B<%N>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "nanoseconds (000000000..999999999)"
msgstr "nanosekundy (000000000..999999999)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%p"
msgstr "B<%p>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "locale's equivalent of either AM or PM; blank if not known"
msgstr ""
"lokalny odpowiednik AM lub PM; w wielu językach pusty (w tym w polskim)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%P"
msgstr "B<%P>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "like %p, but lower case"
msgstr "jak B<%p>, lecz małymi literami"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%q"
msgstr ""

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, fuzzy
#| msgid "day of year (001..366)"
msgid "quarter of year (1..4)"
msgstr "dzień roku (001..366)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%r"
msgstr "B<%r>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "locale's 12-hour clock time (e.g., 11:11:04 PM)"
msgstr "czas w formacie 12-godzinnym (np. 11:11:04 PM)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%R"
msgstr "B<%R>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "24-hour hour and minute; same as %H:%M"
msgstr "godzina i minuty w formacie 24-godzinnym; to samo co B<%H:%M>"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%s"
msgstr "B<%s>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "seconds since 1970-01-01 00:00:00 UTC"
msgstr "liczba sekund od 00:00:00, 1 stycznia 1970 UTC"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%S"
msgstr "B<%S>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "second (00..60)"
msgstr "sekunda (00..60)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%t"
msgstr "B<%t>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "a tab"
msgstr "tabulator"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%T"
msgstr "B<%T>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "time; same as %H:%M:%S"
msgstr "czas; to samo co B<%H:%M:%S>"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%u"
msgstr "B<%u>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "day of week (1..7); 1 is Monday"
msgstr "dzień tygodnia (1..7); 1 to poniedziałek"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%U"
msgstr "B<%U>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "week number of year, with Sunday as first day of week (00..53)"
msgstr "numer tygodnia w roku, niedziela zaczyna tydzień (00..53)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%V"
msgstr "B<%V>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "ISO week number, with Monday as first day of week (01..53)"
msgstr "numer tygodnia ISO, poniedziałek zaczyna tydzień (01..53)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%w"
msgstr "B<%w>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "day of week (0..6); 0 is Sunday"
msgstr "dzień tygodnia (0..6); 0 oznacza niedzielę"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%W"
msgstr "B<%W>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "week number of year, with Monday as first day of week (00..53)"
msgstr "numer tygodnia w roku, poniedziałek zaczyna tydzień (00..53)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%x"
msgstr "B<%x>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "locale's date representation (e.g., 12/31/99)"
msgstr "lokalna reprezentacja daty (np. 19.03.2012)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%X"
msgstr "B<%X>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "locale's time representation (e.g., 23:13:48)"
msgstr "lokalna reprezentacja czasu (np. 23:13:48)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%y"
msgstr "B<%y>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
msgid "last two digits of year (00..99)"
msgstr "dwie ostatnie cyfry roku (00..99)"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#, no-wrap
msgid "%Y"
msgstr "B<%Y>"

