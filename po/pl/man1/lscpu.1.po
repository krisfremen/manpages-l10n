#. type: TH
#, no-wrap
msgid "LSCPU"
msgstr "LSCPU"

#. type: TH
#, no-wrap
msgid "November 2015"
msgstr "listopad 2015"

#. type: TH
#, no-wrap
msgid "util-linux"
