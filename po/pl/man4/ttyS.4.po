#. type: TH
#, no-wrap
msgid "TTYS"
msgstr "TTYS"

#. type: TH
#, no-wrap
msgid "1992-12-19"
msgstr "19-12-1992"

#. type: TH
#, no-wrap
msgid "Linux"
msgstr "Linux"

#. type: TH
#, no-wrap
msgid "Linux Programmer's Manual"
msgstr "Podręcznik programisty Linuksa"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "NAZWA"

#. type: Plain text
msgid "ttyS - serial terminal lines"
msgstr "ttyS - linie terminali szeregowych"

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "OPIS"

#. type: Plain text
msgid "B<ttyS[0-3]> are character devices for the serial terminal lines."
msgstr "B<ttyS[0-3]> są urządzeniami znakowymi linii terminali szeregowych."

#. type: Plain text
msgid "They are typically created by:"
msgstr "Tworzy się je zazwyczaj poleceniami:"

#. type: Plain text
msgid "mknod -m 660 /dev/ttyS0 c 4 64 # base address 0x3f8"
msgstr "mknod -m 660 /dev/ttyS0 c 4 64 # adres bazowy 0x3f8"

#. type: Plain text
msgid "mknod -m 660 /dev/ttyS1 c 4 65 # base address 0x2f8"
msgstr "mknod -m 660 /dev/ttyS1 c 4 65 # adres bazowy 0x2f8"

#. type: Plain text
msgid "mknod -m 660 /dev/ttyS2 c 4 66 # base address 0x3e8"
msgstr "mknod -m 660 /dev/ttyS2 c 4 66 # adres bazowy 0x3e8"

#. type: Plain text
msgid "mknod -m 660 /dev/ttyS3 c 4 67 # base address 0x2e8"
msgstr "mknod -m 660 /dev/ttyS3 c 4 67 # adres bazowy 0x2e8"

#. type: Plain text
msgid "chown root:tty /dev/ttyS[0-3]"
msgstr "chown root:tty /dev/ttyS[0-3]"

#. type: SH
#, no-wrap
msgid "FILES"
msgstr "PLIKI"

#. type: Plain text
msgid "/dev/ttyS[0-3]"
msgstr "/dev/ttyS[0-3]"

#. type: SH
#, no-wrap
msgid "SEE ALSO"
msgstr "ZOBACZ TAKŻE"

#. type: Plain text
msgid ""
"B<chown>(1), B<mknod>(1), B<tty>(4), B<agetty>(8), B<mingetty>(8), "
"B<setserial>(8)"
msgstr ""
"B<chown>(1), B<mknod>(1), B<tty>(4), B<agetty>(8), B<mingetty>(8), "
"B<setserial>(8)"

#. type: SH
#, no-wrap
msgid "COLOPHON"
msgstr "O STRONIE"

