#. type: TH
#, no-wrap
msgid "BOOTPARAM"
msgstr "BOOTPARAM"

#. type: TH
#, no-wrap
msgid "2015-05-07"
msgstr "2015-05-07"

#. type: TH
#, no-wrap
msgid "Linux"
msgstr "Linux"

#. type: TH
#, no-wrap
