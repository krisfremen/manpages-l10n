msgid "Since glibc 2.10:"
msgstr "Od glibc 2.10:"

#. type: Plain text
msgid "_POSIX_C_SOURCE\\ E<gt>=\\ 200809L"
msgstr "_POSIX_C_SOURCE\\ E<gt>=\\ 200809L"

#. type: TP
#, no-wrap
