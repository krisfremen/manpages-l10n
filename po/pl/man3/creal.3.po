#, no-wrap
msgid "Interface"
msgstr "Interfejs"

#. type: tbl table
#, no-wrap
msgid "Attribute"
msgstr "Atrybut"

#. type: tbl table
#, no-wrap
msgid "Value"
msgstr "Wartość"

#. type: tbl table
#, no-wrap
msgid ""
"B<creal>(),\n"
"B<crealf>(),\n"
"B<creall>()"
msgstr ""
"B<creal>(),\n"
