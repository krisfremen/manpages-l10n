msgstr "B<--group-directories-first>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "group directories before files;"
msgstr "agrupa directorios antes que los ficheros;"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid ""
"can be augmented with a B<--sort> option, but any use of B<--sort>=I<\\,none"
"\\/> (B<-U>) disables grouping"
msgstr ""
"se puede añadir una opción B<--sort>, pero cualquier uso de B<--sort>=I<\\,"
"none\\/> (B<-U>) desactiva la agrupación"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "B<-G>, B<--no-group>"
msgstr "B<-G>, B<--no-group>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "in a long listing, don't print group names"
msgstr ""

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "B<-h>, B<--human-readable>"
msgstr "B<-h>, B<--human-readable>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "with B<-l> and B<-s>, print sizes like 1K 234M 2G etc."
msgstr ""

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "B<--si>"
msgstr "B<--si>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "likewise, but use powers of 1000 not 1024"
msgstr "análogo, pero utiliza potencias de 1000 y no de 1024"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "B<-H>, B<--dereference-command-line>"
msgstr "B<-H>, B<--dereference-command-line>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "follow symbolic links listed on the command line"
msgstr "sigue los enlaces simbólicos en la línea de órdenes"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "B<--dereference-command-line-symlink-to-dir>"
msgstr "B<--dereference-command-line-symlink-to-dir>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "follow each command line symbolic link"
msgstr "sigue cada enlace simbólico en la línea de órdenes"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "that points to a directory"
msgstr "que apunte a un directorio"

