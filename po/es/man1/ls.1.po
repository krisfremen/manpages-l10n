#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "B<-d>, B<--directory>"
msgstr "B<-d>, B<--directory>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "list directories themselves, not their contents"
msgstr "muestra los propios directorios, no el contenido"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "B<-D>, B<--dired>"
msgstr "B<-D>, B<--dired>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "generate output designed for Emacs' dired mode"
msgstr "genera el resultado para el modo 'dired' de Emacs"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "B<-f>"
msgstr "B<-f>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "do not sort, enable B<-aU>, disable B<-ls> B<--color>"
msgstr "no ordena, utiliza B<-aU>, no utiliza B<-ls> B<--color>"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "B<-F>, B<--classify>"
msgstr "B<-F>, B<--classify>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "append indicator (one of */=E<gt>@|) to entries"
msgstr "añade un indicador (uno de */=@|) a las entradas"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "B<--file-type>"
msgstr "B<--file-type>"

#. type: Plain text
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
msgid "likewise, except do not append '*'"
msgstr "similar, pero no añade '*'"

#. type: TP
#: archlinux debian-buster debian-unstable fedora-rawhide mageia-cauldron
#: opensuse-tumbleweed
#, no-wrap
msgid "B<--format>=I<\\,WORD\\/>"
msgstr "B<--format>=I<\\,PALABRA\\/>"

