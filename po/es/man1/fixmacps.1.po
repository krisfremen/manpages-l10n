"fixmacps(1), fixpsditps(1), fixpspps(1), fixtpps(1), fixwfwps(1), "
"fixwpps(1), fixwwps(1), extractres(1), includeres(1)"
msgstr ""
"psbook(1), psselect(1), pstops(1), epsffit(1), psnup(1), psresize(1), "
