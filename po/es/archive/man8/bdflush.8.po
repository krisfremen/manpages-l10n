#. type: Plain text
#: man8/bdflush.8:6
msgid "bdflush - kernel daemon to flush dirty buffers back to disk."
msgstr "bdflush - demonio del núcleo para vaciar buffers sucios al disco."

#. type: SH
#: man8/bdflush.8:6
#, no-wrap
msgid "SYNOPSIS"
msgstr "SINOPSIS"

#. type: Plain text
#: man8/bdflush.8:8
msgid "B<bdflush [opt]>"
