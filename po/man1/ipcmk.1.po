#. type: TH
#, no-wrap
msgid "IPCMK"
msgstr "IPCMK"

#. type: TH
#, no-wrap
msgid "March 2008"
msgstr ""

#. type: TH
#, no-wrap
msgid "util-linux"
msgstr ""

#. type: TH
#, no-wrap
msgid "User Commands"
msgstr "Dienstprogramme für Benutzer"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
msgid "ipcmk - create various ipc resources"
msgstr ""

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
msgid "B<ipcmk -h>"
msgstr ""

#. type: Plain text
msgid "B<ipcmk> [B<-M> I<size>] [B<-S> I<nsems>] [B<-Q>] [B<-p> I<mode>]"
msgstr ""

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
msgid ""
"B<ipcmk> allows you to create shared memory segments, message queues or "
"semaphore arrays."
msgstr ""

#. type: SH
#, no-wrap
msgid "OPTIONS"
msgstr "OPTIONEN"

#. type: TP
#, no-wrap
msgid "Resources may be specified as follows:"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-M>I< size>"
msgstr ""

#. type: Plain text
msgid "shared memory segment of size I<size>"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-S>I< nsems>"
msgstr ""

#. type: Plain text
msgid "semaphore array with I<nsems> elements"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-Q>"
msgstr ""

#. type: Plain text
msgid "message queue"
msgstr ""

#. type: TP
#, no-wrap
msgid "Other options"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-p>I< mode>"
msgstr ""

#. type: Plain text
msgid "permission for the resource (default is 0644)"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-h>"
msgstr "B<-h>"

#. type: Plain text
msgid "display a short help message and exit"
msgstr ""

#. type: SH
#, no-wrap
msgid "SEE ALSO"
msgstr "SIEHE AUCH"

#. type: Plain text
msgid "B<ipcrm>(1), B<ipcs>(1)"
msgstr ""

#. type: SH
#, no-wrap
msgid "AUTHOR"
msgstr "AUTOR"

#. type: Plain text
msgid "Hayden A. James (hayden.james@gmail.com)"
msgstr ""

#. type: SH
#, no-wrap
msgid "AVAILABILITY"
msgstr "VERFÜGBARKEIT"

#. type: Plain text
msgid ""
"The ipcmk command is part of the util-linux package and is available from "
"ftp://ftp.kernel.org/pub/linux/utils/util-linux/."
msgstr ""
