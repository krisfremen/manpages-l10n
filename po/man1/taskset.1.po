msgid "is processors #0 and #1,"
msgstr "sind die Prozessoren #0 und #1,"

#. type: TP
#, no-wrap
msgid "B<0xFFFFFFFF>"
msgstr "B<0xFFFFFFFF>"

#. type: Plain text
#, fuzzy
