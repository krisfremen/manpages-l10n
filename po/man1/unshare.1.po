#. type: TH
#, no-wrap
msgid "UNSHARE"
msgstr "UNSHARE"

#. type: TH
#, no-wrap
msgid "October 2008"
msgstr ""

#. type: TH
#, no-wrap
msgid "util-linux"
msgstr ""

#. type: TH
#, no-wrap
msgid "User Commands"
msgstr "Dienstprogramme für Benutzer"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
msgid "unshare - run program with some namespaces unshared from parent"
msgstr ""

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
msgid "B<unshare> [I<options>] program [I<arguments>]"
msgstr ""

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
msgid ""
"Unshares specified namespaces from parent process and then executes "
"specified program. Unshareable namespaces are:"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<mount namespace>"
msgstr ""

#. type: Plain text
msgid ""
"mounting and unmounting filesystems will not affect rest of the system "
"(B<CLONE_NEWNS> flag),"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<UTS namespace>"
msgstr ""

#. type: Plain text
msgid ""
"setting hostname, domainname will not affect rest of the system "
"(B<CLONE_NEWUTS> flag),"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<IPC namespace>"
msgstr ""

#. type: Plain text
msgid ""
"process will have independent namespace for System V message queues, "
"semaphore sets and shared memory segments (B<CLONE_NEWIPC> flag),"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<network namespace>"
msgstr ""

#. type: Plain text
msgid ""
"process will have independent IPv4 and IPv6 stacks, IP routing tables, "
"firewall rules, the I</proc/net> and I</sys/class/net> directory trees, "
"sockets etc. (B<CLONE_NEWNET> flag)."
msgstr ""

#. type: TP
#, no-wrap
msgid "See the clone(2) for exact semantics of the flags."
msgstr ""

#. type: SH
#, no-wrap
msgid "OPTIONS"
msgstr "OPTIONEN"

#. type: TP
#, no-wrap
msgid "B<-h>,B< --help>"
msgstr ""

#. type: Plain text
msgid "Print a help message,"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-m>,B< --mount>"
msgstr ""

#. type: Plain text
msgid "Unshare the mount namespace,"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-u>,B< --uts>"
msgstr ""

#. type: Plain text
msgid "Unshare the UTS namespace,"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-i>,B< --ipc>"
msgstr ""

#. type: Plain text
msgid "Unshare the IPC namespace,"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-n>,B< --net>"
msgstr ""

#. type: Plain text
msgid "Unshare the network namespace."
msgstr ""

#. type: SH
#, no-wrap
msgid "NOTES"
msgstr "ANMERKUNGEN"

#. type: Plain text
msgid ""
"The unshare command drops potential privileges before executing the target "
"program. This allows to setuid unshare."
msgstr ""

#. type: SH
#, no-wrap
msgid "SEE ALSO"
msgstr "SIEHE AUCH"

#. type: Plain text
msgid "unshare(2), clone(2)"
msgstr ""

#. type: SH
#, no-wrap
msgid "BUGS"
msgstr "FEHLER"

#. type: Plain text
msgid "None known so far."
msgstr ""

#. type: SH
#, no-wrap
msgid "AUTHOR"
msgstr "AUTOR"

#. type: Plain text
msgid "Mikhail Gusarov E<lt>dottedmag@dottedmag.netE<gt>"
msgstr ""

#. type: SH
#, no-wrap
msgid "AVAILABILITY"
msgstr "VERFÜGBARKEIT"

