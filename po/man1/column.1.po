#, no-wrap
msgid "ls -l \\&| sed 1d) \\&| column -t"
msgstr ""

#. type: Plain text
#: debian-unstable
msgid "E<.Xr colrm 1>, E<.Xr ls 1>, E<.Xr paste 1>, E<.Xr sort 1>"
msgstr ""

#. type: Plain text
#: debian-unstable
msgid "The E<.Nm> command appeared in E<.Bx 4.3 Reno>."
msgstr ""

