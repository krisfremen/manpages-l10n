msgstr ""

#. type: SH
#, no-wrap
msgid "SEE ALSO"
msgstr "SIEHE AUCH"

#. type: Plain text
msgid "B<taskset>(1), B<nice>(1), B<renice>(1)"
msgstr "B<taskset>(1), B<nice>(1), B<renice>(1)"

#. type: Plain text
msgid ""
"See B<sched_setscheduler>(2)  for a description of the Linux scheduling "
"scheme."
msgstr ""

#. type: SH
#, no-wrap
msgid "AVAILABILITY"
msgstr "VERFÜGBARKEIT"

#. type: Plain text
msgid ""
