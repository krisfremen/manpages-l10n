msgid "UUIDGEN"
msgstr "UUIDGEN"

#. type: TH
#: archlinux
#, no-wrap
msgid "June 2011"
msgstr "Juni 2011"

#. type: TH
#: archlinux
#, no-wrap
msgid "util-linux"
