#. type: TH
#, no-wrap
msgid "BASENAME"
msgstr "BASENAME"

#. type: TH
#, no-wrap
msgid "October 2012"
msgstr "Oktober 2012"

#. type: TH
#, no-wrap
msgid "GNU coreutils 8.20"
msgstr "GNU coreutils 8.20"

#. type: TH
#, no-wrap
msgid "User Commands"
msgstr "Dienstprogramme für Benutzer"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
msgid "basename - strip directory and suffix from filenames"
msgstr "basename - Verzeichnis und Suffix von Dateinamen entfernen"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
msgid "B<basename> I<NAME >[I<SUFFIX>]"
msgstr "B<basename> I<NAME >[I<SUFFIX>]"

#. type: Plain text
#, fuzzy
#| msgid "B<basename> I<OPTION>"
msgid "B<basename> I<OPTION>... I<NAME>..."
msgstr "B<basename> I<OPTION>"

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
msgid ""
"Print NAME with any leading directory components removed.  If specified, "
"also remove a trailing SUFFIX."
msgstr ""
"Den NAMEn ohne führende Verzeichnisse ausgeben. Wenn angegeben, auch SUFFIX "
"entfernen."

#. type: TP
#, fuzzy, no-wrap
#| msgid "B<-a>, B<--all>"
msgid "B<-a>, B<--multiple>"
msgstr "B<-a>, B<--all>"

#. type: Plain text
msgid "support multiple arguments and treat each as a NAME"
msgstr ""

#. type: TP
#, fuzzy, no-wrap
#| msgid "B<-S>, B<--suffix>=I<SUFFIX>"
msgid "B<-s>, B<--suffix>=I<SUFFIX>"
msgstr "B<-S>, B<--suffix>=I<ENDUNG>"

#. type: Plain text
msgid "remove a trailing SUFFIX"
msgstr ""

#. type: TP
#, fuzzy, no-wrap
#| msgid "B<-v>, B<--verbose>"
msgid "B<-z>, B<--zero>"
msgstr "B<-v>, B<--verbose>"

#. type: Plain text
#, fuzzy
#| msgid "do not output the trailing newline"
msgid "separate output with NUL rather than newline"
msgstr "Keinen abschließenden Zeilenumbruch ausgeben"

#. type: TP
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
msgid "display this help and exit"
msgstr "Diese Hilfe anzeigen und beenden"

#. type: TP
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
msgid "output version information and exit"
msgstr "Versionsinformation anzeigen und beenden"

#. type: SH
#, no-wrap
msgid "EXAMPLES"
msgstr "BEISPIELE"

#. type: TP
#, no-wrap
msgid "basename /usr/bin/sort"
msgstr "basename /usr/bin/sort"

#. type: Plain text
msgid "-E<gt> \"sort\""
msgstr ""

#. type: TP
#, no-wrap
msgid "basename include/stdio.h .h"
msgstr "basename include/stdio.h .h"

#. type: Plain text
#, fuzzy
#| msgid "Output \"stdio\"."
msgid "-E<gt> \"stdio\""
msgstr "Ausgabe: »stdio«."

#. type: TP
#, fuzzy, no-wrap
#| msgid "basename include/stdio.h .h"
msgid "basename B<-s> .h include/stdio.h"
msgstr "basename include/stdio.h .h"

#. type: TP
#, no-wrap
msgid "basename B<-a> any/str1 any/str2"
msgstr ""

#. type: Plain text
msgid "-E<gt> \"str1\" followed by \"str2\""
msgstr ""

