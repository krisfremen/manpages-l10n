#| msgid "B<fallocate> [B<-n>] [B<-o> I<offset>] B<-l> I<length> I<filename>"
msgid ""
"B<fallocate> [B<-n>] [B<-p>] [B<-c>] [B<-z>] [B<-o> I<offset>] B<-l> "
