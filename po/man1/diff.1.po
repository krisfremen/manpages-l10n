#. type: TH
#: archlinux
#, no-wrap
msgid "GNU"
msgstr "GNU"

#. type: TH
#: archlinux
#, no-wrap
msgid "December 2018"
msgstr "Dezember 2018"

#. type: TH
#: archlinux
#, no-wrap
msgid "diffutils 3.7"
msgstr "diffutils 3.7"

#. type: TH
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "User Commands"
msgstr "Dienstprogramme für Benutzer"

#. type: SH
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
#: archlinux
msgid "GNU diff - compare files line by line"
msgstr "GNU diff - Dateien zeilenweise vergleichen"

#. type: SH
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "B<diff> [I<OPTION>]... I<FILES>"
msgstr "B<diff> [I<OPTION>]… I<DATEIEN>"

#. type: SH
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "Compare FILES line by line."
msgstr "DATEIEN zeilenweise vergleichen"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""
"Die obligatorischen Argumente für Optionen sind für deren Kurz- und Langform "
"gleich."

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--normal>"
msgstr "B<--normal>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "output a normal diff (the default)"
msgstr "Ein normales Diff ausgeben (Voreinstellung)"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-q>, B<--brief>"
msgstr "B<-q>, B<--brief>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "report only when files differ"
msgstr "Nur melden, wenn sich Dateien unterscheiden"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-s>, B<--report-identical-files>"
msgstr "B<-s>, B<--report-identical-files>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "report when two files are the same"
msgstr "Melden, wenn zwei Dateien gleich sind"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-c>, B<-C> NUM, B<--context>[=I<NUM>]"
msgstr "B<-c>, B<-C> ANZAHL, B<--context>[=I<ANZAHL>]"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "output NUM (default 3) lines of copied context"
msgstr "ANZAHL Zeilen (Vorgabe 3) des kopierten Umfelds ausgeben"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-u>, B<-U> NUM, B<--unified>[=I<NUM>]"
msgstr "B<-u>, B<-U> ANZAHL, B<--unified>[=I<ANZAHL>]"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "output NUM (default 3) lines of unified context"
msgstr "ANZAHL Zeilen (Vorgabe 3) des einheitlichen Umfelds ausgeben"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-e>, B<--ed>"
msgstr "B<-e>, B<--ed>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "output an ed script"
msgstr "Ein Ed-Skript ausgeben"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-n>, B<--rcs>"
msgstr "B<-n>, B<--rcs>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "output an RCS format diff"
msgstr "Ein Diff im RCS-Format ausgeben"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-y>, B<--side-by-side>"
msgstr "B<-y>, B<--side-by-side>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "output in two columns"
msgstr "Zwei Spalten ausgeben"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-W>, B<--width>=I<NUM>"
msgstr "B<-W>, B<--width>=I<ANZAHL>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "output at most NUM (default 130) print columns"
msgstr "Höchstens ANZAHL Spalten (Vorgabe 130) ausgeben"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--left-column>"
msgstr "B<--left-column>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "output only the left column of common lines"
msgstr "Nur die linke Spalte von gemeinsamen Zeilen ausgeben"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--suppress-common-lines>"
msgstr "B<--suppress-common-lines>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "do not output common lines"
msgstr "Gemeinsame Zeilen nicht ausgeben"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-p>, B<--show-c-function>"
msgstr "B<-p>, B<--show-c-function>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "show which C function each change is in"
msgstr "Für jede Änderung die entsprechende C-Funktion zeigen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-F>, B<--show-function-line>=I<RE>"
msgstr "B<-F>, B<--show-function-line>=I<REGULÄRER_AUSDRUCK>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "show the most recent line matching RE"
msgstr "Die letzte Zeile zeigen auf die REGULÄRER_AUSDRUCK passt"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--label> LABEL"
msgstr "B<--label> BESCHRIFTUNG"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "use LABEL instead of file name and timestamp (can be repeated)"
msgstr ""
"BESCHRIFTUNG an Stelle des Dateinamens und Zeitstempels benutzen (kann "
"wiederholt werden)"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-t>, B<--expand-tabs>"
msgstr "B<-t>, B<--expand-tabs>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "expand tabs to spaces in output"
msgstr "Tabulatoren in der Ausgabe durch Leerzeichen ersetzen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-T>, B<--initial-tab>"
msgstr "B<-T>, B<--initial-tab>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "make tabs line up by prepending a tab"
msgstr "Tabulatoren durch Voranstellen von Tabulatoren ausgleichen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--tabsize>=I<NUM>"
msgstr "B<--tabsize>=I<ANZAHL>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "tab stops every NUM (default 8) print columns"
msgstr ""
"Tabulator-Stopps befinden sich nach je ANZAHL (Vorgabe 8) Ausgabespalten"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--suppress-blank-empty>"
msgstr "B<--suppress-blank-empty>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "suppress space or tab before empty output lines"
msgstr "Leerzeichen oder Tabulatoren vor leeren Ausgabezeilen unterdrücken"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-l>, B<--paginate>"
msgstr "B<-l>, B<--paginate>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "pass output through 'pr' to paginate it"
msgstr "Die Ausgabe durch »pr« leiten, um die Seiten zu nummerieren"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-r>, B<--recursive>"
msgstr "B<-r>, B<--recursive>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "recursively compare any subdirectories found"
msgstr "Alle gefundenen Unterverzeichnisse rekursiv vergleichen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--no-dereference>"
msgstr "B<--no-dereference>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "don't follow symbolic links"
msgstr "Symbolischen Links nicht folgen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-N>, B<--new-file>"
msgstr "B<-N>, B<--new-file>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "treat absent files as empty"
msgstr "Fehlende Dateien als leer behandeln"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--unidirectional-new-file>"
msgstr "B<--unidirectional-new-file>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "treat absent first files as empty"
msgstr "Fehlende erste Dateien als leer behandeln"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--ignore-file-name-case>"
msgstr "B<--ignore-file-name-case>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "ignore case when comparing file names"
msgstr "Ignoriert beim Vergleich von Dateinamen Groß- und Kleinschreibung"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--no-ignore-file-name-case>"
msgstr "B<--no-ignore-file-name-case>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "consider case when comparing file names"
msgstr "Beachtet beim Vergleich von Dateinamen Groß- und Kleinschreibung"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-x>, B<--exclude>=I<PAT>"
msgstr "B<-x>, B<--exclude>=I<MUSTER>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "exclude files that match PAT"
msgstr "Dateien ausschließen, die auf MUSTER passen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-X>, B<--exclude-from>=I<FILE>"
msgstr "B<-X>, B<--exclude-from>=I<DATEI>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "exclude files that match any pattern in FILE"
msgstr "Dateien ausschließen, die auf irgendein Muster in DATEI passen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-S>, B<--starting-file>=I<FILE>"
msgstr "B<-S>, B<--starting-file>=I<DATEI>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "start with FILE when comparing directories"
msgstr "Beim Vergleich von Verzeichnissen mit DATEI beginnen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--from-file>=I<FILE1>"
msgstr "B<--from-file>=I<DATEI1>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "compare FILE1 to all operands; FILE1 can be a directory"
msgstr ""
"DATEI1 mit allen Operanden vergleichen. DATEI1 kann ein Verzeichnis sein."

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--to-file>=I<FILE2>"
msgstr "B<--to-file>=I<DATEI2>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "compare all operands to FILE2; FILE2 can be a directory"
msgstr ""
"Alle Operanden mit DATEI2 vergleichen. DATEI2 kann ein Verzeichnis sein."

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-i>, B<--ignore-case>"
msgstr "B<-i>, B<--ignore-case>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "ignore case differences in file contents"
msgstr "Ignoriert unterschiedliche Groß- und Kleinschreibung im Dateiinhalt"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-E>, B<--ignore-tab-expansion>"
msgstr "B<-E>, B<--ignore-tab-expansion>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "ignore changes due to tab expansion"
msgstr "Änderungen auf Grund von Tabulator-Expansion ignorieren"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-Z>, B<--ignore-trailing-space>"
msgstr "B<-Z>, B<--ignore-trailing-space>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "ignore white space at line end"
msgstr "Alle Leerräume am Zeilenende ignorieren"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-b>, B<--ignore-space-change>"
msgstr "B<-b>, B<--ignore-space-change>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "ignore changes in the amount of white space"
msgstr "Änderungen der Anzahl von Leerräumen ignorieren"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-w>, B<--ignore-all-space>"
msgstr "B<-w>, B<--ignore-all-space>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "ignore all white space"
msgstr "Alle Leerräume ignorieren"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-B>, B<--ignore-blank-lines>"
msgstr "B<-B>, B<--ignore-blank-lines>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "ignore changes where lines are all blank"
msgstr "Änderungen ignorieren, die nur leere Zeilen betreffen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-I>, B<--ignore-matching-lines>=I<RE>"
msgstr "B<-I>, B<--ignore-matching-lines>=I<REGULÄRER_AUSDRUCK>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "ignore changes where all lines match RE"
msgstr "Änderungen ignorieren, deren Zeilen alle auf REGULÄRER_AUSDRUCK passen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-a>, B<--text>"
msgstr "B<-a>, B<--text>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "treat all files as text"
msgstr "Alle Dateien als Text behandeln"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--strip-trailing-cr>"
msgstr "B<--strip-trailing-cr>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "strip trailing carriage return on input"
msgstr "Wagenrücklauf (Carriage Return) am Ende der Eingabe entfernen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-D>, B<--ifdef>=I<NAME>"
msgstr "B<-D>, B<--ifdef>=I<NAME>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "output merged file with '#ifdef NAME' diffs"
msgstr "Zusammengefügte Datei mit »#ifdef NAME«-Diffs ausgeben"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--GTYPE-group-format>=I<GFMT>"
msgstr "B<--GTYPE-group-format>=I<GFMT>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "format GTYPE input groups with GFMT"
msgstr "GTYPE-Eingabegruppen mit GFMT formatieren"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--line-format>=I<LFMT>"
msgstr "B<--line-format>=I<LFMT>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "format all input lines with LFMT"
msgstr "Alle Eingabezeilen mit LFMT formatieren"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--LTYPE-line-format>=I<LFMT>"
msgstr "B<--LTYPE-line-format>=I<LFMT>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "format LTYPE input lines with LFMT"
msgstr "Alle LTYPE-Eingabezeilen mit LFMT formatieren"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "These format options provide fine-grained control over the output"
msgstr "Mit den folgenden Optionen, einer Verallgemeinerung von"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "of diff, generalizing B<-D>/--ifdef."
msgstr ""
"B<-D>/--ifdef, können Sie die Formatierung der Ausgabe von diff detailliert "
"festlegen."

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "LTYPE is 'old', 'new', or 'unchanged'."
msgstr "LTYPE kann »old«, »new« oder »unchanged« sein."

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "GTYPE is LTYPE or 'changed'."
msgstr "GTYPE ist LTYPE oder »changed«."

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "GFMT (only) may contain:"
msgstr "GFMT darf (nur) Folgendes enthalten:"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "%E<lt>"
msgstr "%E<lt>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "lines from FILE1"
msgstr "Zeilen aus DATEI1"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "%E<gt>"
msgstr "%E<gt>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "lines from FILE2"
msgstr "Zeilen aus DATEI2"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "%="
msgstr "%="

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "lines common to FILE1 and FILE2"
msgstr "Gemeinsame Zeilen von DATEI1 und DATEI2"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "%[-][WIDTH][.[PREC]]{doxX}LETTER"
msgstr "%[-][BREITE][.[PRÄZ]]{doxX}BUCHSTABE"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "printf-style spec for LETTER"
msgstr "Druckstil-Spezifikation für BUCHSTABE"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "LETTERs are as follows for new group, lower case for old group:"
msgstr ""
"BUCHSTABEn sind für neue Gruppe, Kleinschreibung und alte Gruppe wie folgt:"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "F"
msgstr "F"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "first line number"
msgstr "Erste Zeilennummer"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "L"
msgstr "L"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "last line number"
msgstr "Letzte Zeilennummer"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "N"
msgstr "N"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "number of lines = L-F+1"
msgstr "Anzahl der Zeilen = L-F+1"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "E"
msgstr "E"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "F-1"
msgstr "F-1"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "M"
msgstr "M"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "L+1"
msgstr "L+1"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "%(A=B?T:E)"
msgstr "%(A=B?T:E)"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "if A equals B then T else E"
msgstr "Wenn A gleich B ist, dann T, sonst E"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "LFMT (only) may contain:"
msgstr "LFMT darf (nur) Folgendes enthalten:"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "%L"
msgstr "%L"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "contents of line"
msgstr "Inhalt der Zeile"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "%l"
msgstr "%l"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "contents of line, excluding any trailing newline"
msgstr "Inhalt der Zeile, außer irgendwelchen führenden Zeilenumbrüchen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "%[-][WIDTH][.[PREC]]{doxX}n"
msgstr "%[-][BREITE][.[PRÄZ]]{doxX}n"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "printf-style spec for input line number"
msgstr "Druckstil-Spezifikation für Eingabe-Zeilennummer"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "Both GFMT and LFMT may contain:"
msgstr "Sowohl GFMT als auch LFMT dürfen Folgendes enthalten:"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "%%"
msgstr "%%"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "%"
msgstr "%"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "%c'C'"
msgstr "%c»C«"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "the single character C"
msgstr "Das einzelne Zeichen C"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "%c'\\eOOO'"
msgstr "%c»\\eOOO«"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "the character with octal code OOO"
msgstr "Das Zeichen mit dem oktalen Code 000"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "C"
msgstr "C"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "the character C (other characters represent themselves)"
msgstr "Das Zeichen C (andere Zeichen stellen sich selbst dar)"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<-d>, B<--minimal>"
msgstr "B<-d>, B<--minimal>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "try hard to find a smaller set of changes"
msgstr "Intensive Suche nach einer kleineren Menge von Änderungen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--horizon-lines>=I<NUM>"
msgstr "B<--horizon-lines=>I<ANZAHL>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "keep NUM lines of the common prefix and suffix"
msgstr "ANZAHL Zeilen des üblichen Präfix und der Endung behalten"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--speed-large-files>"
msgstr "B<--speed-large-files>"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "assume large files and many scattered small changes"
msgstr "Große Dateien und viele vereinzelte kleine Änderungen annehmen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--color>[=I<WHEN>]"
msgstr "B<--color>[=I<WANN>]"

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid ""
"colorize the output; WHEN can be 'never', 'always', or 'auto' (the default)"
msgstr ""
"Farben bei der Ausgabe verwenden; WANN kann die Werte »never«, »always« oder "
"»auto« (Voreinstellung) annehmen"

#. type: TP
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "B<--palette>=I<PALETTE>"
msgstr "B<--palette>=I<PALETTE>"

#. type: Plain text
#: archlinux debian-unstable
msgid ""
"the colors to use when B<--color> is active; PALETTE is a colon-separated "
"list of terminfo capabilities"
msgstr ""
"Die Farben, die verwandt werden sollen, wenn B<--color> aktiv ist. PALETTE "
"ist eine durch Doppelpunkte getrennte Liste von »terminfo capabilities«"

