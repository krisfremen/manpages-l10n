msgstr "Dienstprogramme für Benutzer"

#. type: SH
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
#, fuzzy
msgid "who - show who is logged on"
msgstr "who - Anzeige, wer eingeloggt ist"

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#, fuzzy
msgid "B<who> [I<OPTION>]... [ I<FILE | ARG1 ARG2 >]"
msgstr "B<who> [I<OPTION>]... [ I<DATEI | ARG1 ARG2 >]"

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
#, fuzzy
msgid "Print information about users who are currently logged in."
msgstr "Informationen über gerade angemeldete Benutzer ausgeben."

#. type: TP
#, fuzzy, no-wrap
msgid "B<-a>, B<--all>"
msgstr "B<-a>, B<--all>"

#. type: Plain text
#, fuzzy
msgid "same as B<-b> B<-d> B<--login> B<-p> B<-r> B<-t> B<-T> B<-u>"
msgstr "dasselbe wie B<-b> B<-d> B<--login> B<-p> B<-r> B<-t> B<-T> B<-u>"

#. type: TP
#, fuzzy, no-wrap
msgid "B<-b>, B<--boot>"
msgstr "B<-b>, B<--boot>"

#. type: Plain text
#, fuzzy
msgid "time of last system boot"
msgstr "Zeit des letzten Rechnerstarts (»system boot«)"

#. type: TP
#, fuzzy, no-wrap
msgid "B<-d>, B<--dead>"
msgstr "B<-d>, B<--dead>"

#. type: Plain text
#, fuzzy
msgid "print dead processes"
msgstr "tote Prozesse ausgeben"

#. type: TP
#, fuzzy, no-wrap
msgid "B<-H>, B<--heading>"
msgstr "B<-H>, B<--heading>"

#. type: Plain text
#, fuzzy
msgid "print line of column headings"
msgstr "Kopfzeile mit Spaltenbezeichnungen ausgeben"

#. type: TP
#, fuzzy, no-wrap
msgid "B<--ips>"
msgstr "B<--ips>"

#. type: Plain text
#, fuzzy
msgid ""
"print ips instead of hostnames. with B<--lookup>, canonicalizes based on "
"stored IP, if available, rather than stored hostname"
msgstr ""
"print ips instead of hostnames. with B<--lookup>, canonicalizes based on "
"stored IP, if available, rather than stored hostname"

#. type: TP
#, fuzzy, no-wrap
msgid "B<-l>, B<--login>"
msgstr "B<-l>, B<--login>"

#. type: Plain text
#, fuzzy
msgid "print system login processes"
msgstr "Login‐Prozesse des Systems ausgeben"

#. type: TP
#, fuzzy, no-wrap
msgid "B<--lookup>"
msgstr "B<--lookup>"

#. type: Plain text
#, fuzzy
msgid "attempt to canonicalize hostnames via DNS"
msgstr "versuchen, den Rechnernamen mittels DNS zu kanonifizieren"

#. type: TP
#, fuzzy, no-wrap
msgid "B<-m>"
msgstr "B<-m>"

#. type: Plain text
#, fuzzy
msgid "only hostname and user associated with stdin"
msgstr "nur Rechnernamen und Benutzer, die die Standardeingabe verwenden"

#. type: TP
#, fuzzy, no-wrap
msgid "B<-p>, B<--process>"
msgstr "B<-p>, B<--process>"

#. type: Plain text
#, fuzzy
msgid "print active processes spawned by init"
msgstr "aktive Prozesse ausgeben, die von init aufgerufen wurden"

#. type: TP
#, fuzzy, no-wrap
msgid "B<-q>, B<--count>"
msgstr "B<-q>, B<--count>"

#. type: Plain text
#, fuzzy
msgid "all login names and number of users logged on"
msgstr "alle Loginnamen und Anzahl der angemeldeten Benutzer"

#. type: TP
#, fuzzy, no-wrap
msgid "B<-r>, B<--runlevel>"
msgstr "B<-r>, B<--runlevel>"

#. type: Plain text
#, fuzzy
msgid "print current runlevel"
msgstr "aktuellen Runlevel ausgeben"

#. type: TP
#, fuzzy, no-wrap
msgid "B<-s>, B<--short>"
msgstr "B<-s>, B<--short>"

#. type: Plain text
#, fuzzy
msgid "print only name, line, and time (default)"
msgstr "nur Namen, Leitung und Zeit ausgeben (Vorgabe)"

#. type: TP
#, fuzzy, no-wrap
msgid "B<-t>, B<--time>"
msgstr "B<-t>, B<--time>"

#. type: Plain text
#, fuzzy
msgid "print last system clock change"
msgstr "das letztmalige Stellen der Systemuhr ausgeben"

#. type: TP
#, fuzzy, no-wrap
msgid "B<-T>, B<-w>, B<--mesg>"
msgstr "B<-T>, B<-w>, B<--mesg>"

#. type: Plain text
#, fuzzy
msgid "add user's message status as +, - or ?"
msgstr "den Message‐Status des Benutzers als +, - or ? hinzufügen"

#. type: TP
#, fuzzy, no-wrap
msgid "B<-u>, B<--users>"
msgstr "B<-u>, B<--users>"

#. type: Plain text
#, fuzzy
msgid "list users logged in"
msgstr "angemeldete Benutzer anzeigen"

#. type: TP
#, fuzzy, no-wrap
msgid "B<--message>"
msgstr "B<--message>"

#. type: Plain text
#, fuzzy
msgid "same as B<-T>"
msgstr "dasselbe wie B<-T>"

