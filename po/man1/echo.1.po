#. type: Plain text
msgid "B<echo> [I<SHORT-OPTION>]... [I<STRING>]..."
msgstr ""

#. type: Plain text
msgid "B<echo> I<LONG-OPTION>"
msgstr ""

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
msgid "Echo the STRING(s) to standard output."
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-n>"
msgstr ""

#. type: Plain text
msgid "do not output the trailing newline"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-e>"
msgstr ""

#. type: Plain text
msgid "enable interpretation of backslash escapes"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-E>"
msgstr ""

#. type: Plain text
msgid "disable interpretation of backslash escapes (default)"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
msgid "display this help and exit"
msgstr "Diese Hilfe anzeigen und beenden"

#. type: TP
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
msgid "output version information and exit"
msgstr "Versionsinformation anzeigen und beenden"

#. type: Plain text
msgid "If B<-e> is in effect, the following sequences are recognized:"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\e\\e"
