msgid "backslash"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\ea"
msgstr ""

#. type: Plain text
msgid "alert (BEL)"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\eb"
msgstr ""

#. type: Plain text
msgid "backspace"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\ec"
msgstr ""

#. type: Plain text
msgid "produce no further output"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\ee"
msgstr ""

#. type: Plain text
msgid "escape"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\ef"
msgstr ""

#. type: Plain text
msgid "form feed"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\en"
msgstr ""

#. type: Plain text
msgid "new line"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\er"
msgstr ""

#. type: Plain text
msgid "carriage return"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\et"
msgstr ""

#. type: Plain text
msgid "horizontal tab"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\ev"
msgstr ""

#. type: Plain text
msgid "vertical tab"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\e0NNN"
msgstr ""

#. type: Plain text
msgid "byte with octal value NNN (1 to 3 digits)"
msgstr ""

#. type: TP
#, no-wrap
