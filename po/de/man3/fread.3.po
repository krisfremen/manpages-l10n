#. type: Plain text
#: archlinux mageia-cauldron opensuse-tumbleweed
msgid ""
"The program below demonstrates the use of B<fread>()  by parsing /bin/sh ELF "
"executable in binary mode and printing its magic and class:"
msgstr ""

#. type: Plain text
#: archlinux mageia-cauldron opensuse-tumbleweed
#, no-wrap
msgid ""
"$ B<./a.out>\n"
"ELF magic: 0x7f454c46\n"
"Class: 0x02\n"
msgstr ""

#. type: SS
#: archlinux mageia-cauldron opensuse-tumbleweed
#, no-wrap
msgid "Program source"
msgstr "Programmquelltext"

#. type: Plain text
#: archlinux mageia-cauldron opensuse-tumbleweed
#, no-wrap
msgid ""
"#include E<lt>stdio.hE<gt>\n"
"#include E<lt>stdlib.hE<gt>\n"
msgstr ""
"#include E<lt>stdio.hE<gt>\n"
"#include E<lt>stdlib.hE<gt>\n"

#. type: Plain text
#: archlinux mageia-cauldron opensuse-tumbleweed
#, no-wrap
msgid ""
"int\n"
"main(void)\n"
"{\n"
"    FILE *fp = fopen(\"/bin/sh\", \"rb\");\n"
"    if (!fp) {\n"
"        perror(\"fopen\");\n"
"        return EXIT_FAILURE;\n"
"    }\n"
msgstr ""
"int\n"
"main(void)\n"
"{\n"
"    FILE *fp = fopen(\"/bin/sh\", \"rb\");\n"
"    if (!fp) {\n"
"        perror(\"fopen\");\n"
"        return EXIT_FAILURE;\n"
"    }\n"

#. type: Plain text
#: archlinux mageia-cauldron opensuse-tumbleweed
#, no-wrap
msgid "    unsigned char buffer[4];\n"
msgstr "    unsigned char buffer[4];\n"

#. type: Plain text
#: archlinux mageia-cauldron opensuse-tumbleweed
#, no-wrap
msgid ""
"    size_t ret =\n"
"        fread(buffer, sizeof(buffer) / sizeof(*buffer), sizeof(*buffer),\n"
"              fp);\n"
"    if (ret != sizeof(*buffer)) {\n"
"        fprintf(stderr, \"fread() failed: %zu\\en\", ret);\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
msgstr ""

#. type: Plain text
#: archlinux mageia-cauldron opensuse-tumbleweed
#, no-wrap
msgid ""
"    printf(\"ELF magic: %#04x%02x%02x%02x\\en\", buffer[0], buffer[1],\n"
"           buffer[2], buffer[3]);\n"
msgstr ""

#. type: Plain text
#: archlinux mageia-cauldron opensuse-tumbleweed
#, fuzzy, no-wrap
#| msgid ""
#| "    nbytes = readlink(argv[1], buf, bufsiz);\n"
#| "    if (nbytes == -1) {\n"
#| "        perror(\"readlink\");\n"
#| "        exit(EXIT_FAILURE);\n"
#| "    }\n"
msgid ""
"    ret = fread(buffer, 1, 1, fp);\n"
"    if (ret != 1) {\n"
"        fprintf(stderr, \"fread() failed: %zu\\en\", ret);\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"
msgstr ""
"    nbytes = readlink(argv[1], buf, bufsiz);\n"
"    if (nbytes == -1) {\n"
"        perror(\"readlink\");\n"
"        exit(EXIT_FAILURE);\n"
"    }\n"

#. type: Plain text
#: archlinux mageia-cauldron opensuse-tumbleweed
#, fuzzy, no-wrap
#| msgid "    printf(\"Key ID is %lx\\en\", (long) key);\n"
msgid "    printf(\"Class: %#04x\\en\", buffer[0]);\n"
msgstr "    printf(\"Schlüsselkennung ist %lx\\en\", (long) key);\n"

#. type: Plain text
#: archlinux mageia-cauldron opensuse-tumbleweed
#, no-wrap
msgid "    fclose(fp);\n"
msgstr "    fclose(fp);\n"

#. type: Plain text
#: archlinux mageia-cauldron opensuse-tumbleweed
#, no-wrap
msgid ""
"    exit(EXIT_SUCCESS);\n"
"}\n"
msgstr ""
"    exit(EXIT_SUCCESS);\n"
"}\n"

