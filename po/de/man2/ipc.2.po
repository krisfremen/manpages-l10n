#: archlinux fedora-rawhide opensuse-tumbleweed
#, no-wrap
#| msgid ""
#| "B<int ipc(unsigned int >I<call>B<, int >I<first>B<, int >I<second>B<, int >I<third>B<,>\n"
#| "B<        void *>I<ptr>B<, long >I<fifth>B<);>\n"
msgid ""
"B<int ipc(unsigned int >I<call>B<, int >I<first>B<, unsigned long >I<second"
">B<,>\n"
"B<        unsigned long >I<third>B<, void *>I<ptr>B<, long >I<fifth>B<);>\n"
msgstr ""
"B<int ipc(unsigned int >I<Aufruf>B<, int >I<erster>B<, unsigned long >I<"
