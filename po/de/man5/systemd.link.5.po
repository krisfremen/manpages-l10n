#. type: Plain text
#: archlinux mageia-cauldron
#, no-wrap
msgid ""
"[Match]\n"
"Path=pci-0000:00:1a\\&.0-*\n"
msgstr ""

#. type: Plain text
#: archlinux mageia-cauldron
#, no-wrap
msgid ""
"[Link]\n"
"Name=internet0\n"
msgstr ""

#. type: Plain text
#: archlinux
msgid "B<Example\\ \\&5.\\ \\&/etc/systemd/network/25-wireless\\&.link>"
msgstr ""

#. type: Plain text
#: archlinux mageia-cauldron
msgid ""
"Here\\*(Aqs an overly complex example that shows the use of a large number "
"of [Match] and [Link] settings\\&."
msgstr ""

#. type: Plain text
#: archlinux mageia-cauldron
#, no-wrap
msgid ""
"[Match]\n"
"MACAddress=12:34:56:78:9a:bc\n"
"Driver=brcmsmac\n"
"Path=pci-0000:02:00\\&.0-*\n"
"Type=wlan\n"
"Virtualization=no\n"
"Host=my-laptop\n"
"Architecture=x86-64\n"
msgstr ""

#. type: Plain text
#: archlinux mageia-cauldron
#, no-wrap
msgid ""
"[Link]\n"
"Name=wireless0\n"
"MTUBytes=1450\n"
"BitsPerSecond=10M\n"
"WakeOnLan=magic\n"
"MACAddress=cb:a9:87:65:43:21\n"
msgstr ""

