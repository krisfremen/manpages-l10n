"[Network]\n"
"Tunnel=ipip-tun\n"
"Tunnel=sit-tun\n"
"Tunnel=gre-tun\n"
"Tunnel=vti-tun\n"
msgstr ""
"[Network]\n"
"Tunnel=ipip-tun\n"
"Tunnel=sit-tun\n"
"Tunnel=gre-tun\n"
"Tunnel=vti-tun\n"

#. type: Plain text
#: archlinux debian-bullseye debian-unstable fedora-rawhide mageia-cauldron
