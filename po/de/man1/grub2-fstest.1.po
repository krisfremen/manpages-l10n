#. type: TH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "GRUB-FSTEST"
msgstr "GRUB-FSTEST"

#. type: TH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "Wed Feb 26 2014"
msgstr "Mittwoch, 26. Februar 2014"

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

# FIXME â → a
#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "B<grub-fstest> â Debug tool for GRUB's filesystem driver."
msgstr "B<grub-fstest> - ein Debug-Werkzeug für den GRUB-Dateisystemtreiber"

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "B<grub-fstest> [-c | --diskcount=I<NUM>] [-C | --crypto]"
msgstr "B<grub-fstest> [-c | --diskcount=I<anZAHL>] [-C | --crypto]"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[-d | --debug=I<STRING>] [-K | --zfs-key=I<FILE>|I<prompt>]"
msgstr ""
"[-d | --debug=I<ZEICHENKETTE>] [-K | --zfs-key=I<DATEI>|"
"I<Eingabeaufforderung>]"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[-n | --length=I<NUM>] [-r | --root=I<DEVICE_NAME>]"
msgstr "[-n | --length=I<ANZAHL>] [-r | --root=I<GERÄTENAME>]"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[-s | --skip=I<NUM>] [-u | --uncompress] [-v | --verbose]"
msgstr "[-s | --skip=I<ANZAHL>] [-u | --uncompress] [-v | --verbose]"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "I<IMAGE_PATH> E<lt>blocklist I<FILE> | cat I<FILE> |"
msgstr "I<ABBILDPFAD> E<lt>blocklist I<DATEI> | cat I<DATEI> |"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "cmp I<FILE> I<LOCAL> | cp I<FILE> I<LOCAL> | crc I<FILE> |"
msgstr ""
"cmp I<DATEI> I<LOKALE_DATEI> | cp I<DATEI> I<LOKALE_DATEI> | crc I<DATEI> |"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "hex I<FILE> | ls I<PATH> | xnu_uuid I<DEVICE>E<gt>"
msgstr "hex I<DATEI> | ls I<PFAD> | xnu_uuid I<GERÄT>E<gt>"

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"B<grub-fstest> is a tool for testing GRUB's filesystem drivers.  You should "
"not normally need to run this program."
msgstr ""
"B<grub-fstest> ist ein Werkzeug zum Testen der GRUB-Dateisystemtreiber. Sie "
"sollten dieses Programm normalerweise nicht ausführen müssen."

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "OPTIONS"
msgstr "OPTIONEN"

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<--diskcount>=I<NUM>"
msgstr "B<--diskcount>=I<ANZAHL>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Specify the number of input files."
msgstr "legt die Anzahl der Eingabedateien fest."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<--crypto>"
msgstr "B<--crypto>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Mount cryptographic devices."
msgstr "hängt verschlüsselte Geräte ein."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<--debug>=I<STRING>"
msgstr "B<--debug>=I<ZEICHENKETTE>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Set debug environment variable."
msgstr "setzt die Debug-Umgebungsvariable."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<--zfs-key>=I<FILE>|I<prompt>"
msgstr "B<--zfs-key>=I<DATEI>|I<prompt>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Load ZFS cryptographic key."
msgstr "lädt den ZFS-Kryptoschlüssel."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<--length>=I<NUM>"
msgstr "B<--length>=I<ZAHL>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Handle NUM bytes in output file."
msgstr "verarbeitet die angegebene ANZAHL Byte in der Ausgabedatei."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<--root>=I<DEVICE_NAME>"
msgstr "B<--root>=I<GERÄTENAME>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Set root device."
msgstr "legt das Wurzelgerät fest."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<--skip>=I<NUM>"
msgstr "B<--skip>=I<ANZAHL>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Skip NUM bytes from output file."
msgstr "überspringt die angegebene ANZAHL Byte in der Ausgabedatei."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<--uncompress>"
msgstr "B<--uncompress>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Uncompress data."
msgstr "dekomprimiert Daten."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<--verbose>"
msgstr "B<--verbose>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Print verbose messages."
msgstr "gibt ausführliche Meldungen aus."

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "COMMANDS"
msgstr "BEFEHLE"

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<blocklist> I<FILE>"
msgstr "B<blocklist> I<DATEI>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Display block list of I<FILE>."
msgstr "zeigt die Blockliste der angegebenen I<DATEI> an."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<cat> I<FILE>"
msgstr "B<cat> I<DATEI>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Display I<FILE> on standard output."
msgstr "kopiert die angegebene I<DATEI> in die Standardausgabe."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<cmp> I<FILE> I<LOCAL>"
msgstr "B<cmp> I<DATEI> I<LOKALE_DATEI>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Compare I<FILE> with local file I<LOCAL>."
msgstr "vergleicht die I<DATEI> mit der I<LOKALEN_DATEI>."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<cp> I<FILE> I<LOCAL>"
msgstr "B<cp> I<DATEI> I<LOKALE_DATEI>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Copy I<FILE> to local file I<LOCAL>."
msgstr "kopiert die I<DATEI> zur I<LOKALEN_DATEI>."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<crc> I<FILE>"
msgstr "B<crc> I<DATEI>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Display the CRC-32 checksum of I<FILE>."
msgstr "ermittelt die CRC-32-Prüfsumme der angegebenen i<DATEI>."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<hex> I<FILE>"
msgstr "B<hex> I<DATEI>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Display contents of I<FILE> in hexidecimal."
msgstr "zeigt den Inhalt der angegebenen I<DATEI> in hexadezimaler Form an."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<ls> I<PATH>"
msgstr "B<ls> I<PFAD>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "List files at I<PATH>."
msgstr "listet Dateien im angegebenen I<PFAD> auf."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "B<xnu_uuid> I<DEVICE>"
msgstr "B<xnu_uuid> I<GERÄT>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Display the XNU UUID of I<DEVICE>."
msgstr "zeigt die XNU-UUID des I<GERÄTS> an."

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "SEE ALSO"
msgstr "SIEHE AUCH"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "B<info grub>"
msgstr "B<info grub>"
