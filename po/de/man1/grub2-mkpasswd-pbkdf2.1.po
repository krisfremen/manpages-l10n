#. type: TH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "GRUB-MKPASSWD-PBKDF2"
msgstr "GRUB-MKPASSWD-PBKDF2"

#. type: TH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "Wed Feb 26 2014"
msgstr "Mittwoch, 26. Februar 2014"

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "B<grub-mkpasswd-pbkdf2> \\(em Generate a PBKDF2 password hash."
msgstr "B<grub-mkpasswd-pbkdf2> \\(emeinen PBKDF2-Passwort-Hash erzeugen"

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"B<grub-mkpasswd-pbkdf2> [-c | --iteration-count=I<NUM>] [-l | --"
"buflen=I<NUM>]"
msgstr ""
"B<grub-mkpasswd-pbkdf2> [-c | --iteration-count=I<ZAHL>] [-l | --"
"buflen=I<ZAHL>]"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[-s | --salt=I<NUM>]"
msgstr "[-s | --salt=I<ZAHL>]"

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"B<grub-mkpasswd-pbkdf2> generates a PBKDF2 password string suitable for use "
"in a GRUB configuration file."
msgstr ""
"B<grub-mkpasswd-pbkdf2> erzeugt eine PBKDF2-Passwortzeichenkette, die in "
"einer GRUB-Konfigurationsdatei verwendet werden kann."

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "OPTIONS"
msgstr "OPTIONEN"

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--iteration-count=I<NUM>"
msgstr "--iteration-count=I<ZAHL>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Number of PBKDF2 iterations."
msgstr "legt die Anzahl der PBKDF2-Durchläufe fest."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--buflen=I<NUM>"
msgstr "--buflen=I<ZAHL>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Length of generated hash."
msgstr "gibt die Länge des erzeugten Hashs an."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--salt=I<NUM>"
msgstr "--salt=I<ZAHL>"

# https://de.wikipedia.org/wiki/Salt_(Kryptologie)
#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Length of salt to use."
msgstr "gibt die Salt-Länge (hinzugefügte Zufallszeichenfolge) an."

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "SEE ALSO"
msgstr "SIEHE AUCH"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "B<info grub>"
msgstr "B<info grub>"
