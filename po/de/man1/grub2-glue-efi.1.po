#. type: TH
#: mageia-cauldron
#, no-wrap
msgid "GRUB-GLUE-EFI"
msgstr "GRUB-GLUE-EFI"

#. type: TH
#: mageia-cauldron
#, no-wrap
#| msgid "3 Feb 2001"
msgid "Wed Feb 26 2014"
msgstr "Mittwoch, 26. Februar 2014"

#. type: SH
#: mageia-cauldron
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
#: mageia-cauldron
#| msgid "grub-glue-efi - generate a fat binary for EFI"
msgid "B<grub-glue-efi> \\(em Create an Apple fat EFI binary."
msgstr ""
"B<grub-glue-efi> \\(em eine vereinigte Apple-Binärdatei für EFI erzeugen."

#. type: SH
#: mageia-cauldron
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: mageia-cauldron
msgid ""
"B<grub-glue-efi> E<lt>-3 | --input32=I<FILE>E<gt> E<lt>-6 | --"
"input64=I<FILE>E<gt>"
msgstr ""
"B<grub-glue-efi> E<lt>-3 | --input32=I<DATEI>E<gt> E<lt>-6 | --"
"input64=I<DATEI>E<gt>"

#. type: Plain text
#: mageia-cauldron
msgid "E<lt>-o | --output=I<FILE>E<gt> [-v | --verbose]"
msgstr "E<lt>-o | --output=I<DATEI>E<gt> [-v | --verbose]"

#. type: SH
#: mageia-cauldron
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
#: mageia-cauldron
#| msgid "grub-glue-efi - generate a fat binary for EFI"
msgid "B<grub-glue-efi> creates an Apple fat EFI binary from two EFI binaries."
msgstr ""
"B<grub-glue-efi> erzeugt eine vereinigte Apple-Binärdatei für EFI aus zwei "
"EFI-Binärdateien."

#. type: SH
#: mageia-cauldron
#, no-wrap
msgid "OPTIONS"
msgstr "OPTIONEN"

#. type: TP
#: mageia-cauldron
#, no-wrap
#| msgid "B<-3>, B<--input32>=I<\\,FILE\\/>"
msgid "B<--input32>=I<FILE>"
msgstr "B<--input32>=I<DATEI>"

#. type: Plain text
#: mageia-cauldron
msgid "Read 32-bit binary from I<FILE>."
msgstr "liest den 32-Bit-Teil aus der angegebenen I<DATEI>."

#. type: TP
#: mageia-cauldron
#, no-wrap
#| msgid "B<-6>, B<--input64>=I<\\,FILE\\/>"
msgid "B<--input64>=I<FILE>"
msgstr "B<--input64>=I<DATEI>"

#. type: Plain text
#: mageia-cauldron
msgid "Read 64-bit binary from I<FILE>."
msgstr "liest den 64-Bit-Teil aus der angegebenen I<DATEI>."

#. type: TP
#: mageia-cauldron
#, no-wrap
#| msgid "B<-o>, B<--output>=I<\\,FILE\\/>"
msgid "B<--output>=I<FILE>"
msgstr "B<--output>=I<DATEI>"

#. type: Plain text
#: mageia-cauldron
msgid "Write resulting fat binary to I<FILE>."
msgstr ""
"schreibt das sich ergebende vereinigte Binärdatei in die angegebene I<DATEI>."

#. type: TP
#: mageia-cauldron
#, no-wrap
msgid "B<--verbose>"
msgstr "B<--verbose>"

#. type: Plain text
#: mageia-cauldron
msgid "Print verbose messages."
msgstr "gibt ausführliche Meldungen aus."

#. type: SH
#: mageia-cauldron
#, no-wrap
msgid "SEE ALSO"
msgstr "SIEHE AUCH"

#. type: Plain text
#: mageia-cauldron
#| msgid "B<info grub-glue-efi>"
msgid "B<info grub>"
msgstr "B<info grub>"
