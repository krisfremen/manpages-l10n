#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: archlinux mageia-cauldron
msgid "B<col> [options]"
msgstr "B<col> [Optionen]"

#. #-#-#-#-#  archlinux: col.1.pot (PACKAGE VERSION)  #-#-#-#-#
#. type: SH
#. #-#-#-#-#  debian-buster: col.1.pot (PACKAGE VERSION)  #-#-#-#-#
#. type: Sh
#. #-#-#-#-#  debian-unstable: col.1.pot (PACKAGE VERSION)  #-#-#-#-#
