#. type: TH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "GRUB-KBDCOMP"
msgstr "GRUB-KBDCOMP"

#. type: TH
#: fedora-rawhide mageia-cauldron
#, no-wrap
#| msgid "3 Feb 2001"
msgid "Wed Feb 26 2014"
msgstr "Mittwoch, 26. Februar 2014"

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
#| msgid "grub-kbdcomp - generate a GRUB keyboard layout file"
msgid "B<grub-kbdcomp> \\(em Generate a GRUB keyboard layout file."
msgstr "B<grub-kbdcomp> \\(em eine Tastaturbelegungsdatei für GRUB erzeugen."

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
#| msgid "B<grub-kbdcomp> I<\\,-o OUTPUT CKBMAP_ARGUMENTS\\/>..."
msgid "B<grub-kbdcomp> E<lt>-o | --output=I<FILE>E<gt> I<CKBMAP_ARGUMENTS>"
msgstr "B<grub-kbdcomp> E<lt>-o | --output=I<DATEI>E<gt> I<CKBMAP_ARGUMENTE>"

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
#| msgid ""
#| "grub-kbdcomp processes a X keyboard layout description in B<keymaps>(5)  "
#| "format into a format that can be used by GRUB's B<keymap> command."
msgid ""
"B<grub-kbdcomp> processes an X keyboard layout description in B<keymaps>(5) "
"format into a format that can be used by GRUB's B<keymap> command."
msgstr ""
"B<grub-kbdcomp> wandelt eine X-Tastaturbelegungsbeschreibung im "
"B<keymaps>(5)-Format in ein Format um, das vom GRUB-Befehl B<keymap> "
"verwendet werden kann."

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "OPTIONS"
msgstr "OPTIONEN"

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
#| msgid "B<-o>, B<--output>=I<\\,FILE\\/>"
msgid "B<--output>=I<FILE>"
msgstr "B<--output>=I<DATEI>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Write output to I<FILE>."
msgstr "schreibt die Ausgabe in die angegebene I<DATEI>."

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "SEE ALSO"
msgstr "SIEHE AUCH"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
#| msgid "B<info grub-kbdcomp>"
msgid "B<info grub>"
msgstr "B<info grub>"
