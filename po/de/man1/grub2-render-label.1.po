#. type: TH
#: mageia-cauldron opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "GRUB-RENDER-LABEL"
msgstr "GRUB-RENDER-LABEL"

#. type: TH
#: mageia-cauldron
#, no-wrap
msgid "Wed Feb 26 2014"
msgstr "Mittwoch, 26. Februar 2014"

#. type: SH
#: mageia-cauldron opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
#: mageia-cauldron
msgid "B<grub-render-label> \\(em Render an Apple disk label."
msgstr "B<grub-render-label> \\(em eine Apple-Plattenbezeichnung erzeugen"

#. type: SH
#: mageia-cauldron opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: mageia-cauldron
msgid "B<grub-render-label> [-b | --bgcolor=I<COLOR>] [-c | --color=I<COLOR>]"
msgstr "B<grub-render-label> [-b | --bgcolor=I<FARBE>] [-c | --color=I<FARBE>]"

#. type: Plain text
#: mageia-cauldron
msgid "[-f | --font=I<FILE>] [-i | --input=I<FILE>]"
msgstr "[-f | --font=I<DATEI>] [-i | --input=I<DATEI>]"

#. type: Plain text
#: mageia-cauldron
msgid "[-o | --output=I<FILE>] [-t | --text=I<STRING>]"
msgstr "[-o | --output=I<DATEI>] [-t | --text=I<ZEICHENKETTE>]"

#. type: Plain text
#: mageia-cauldron
msgid "[-v | --verbose]"
msgstr "[-v | --verbose]"

#. type: SH
#: mageia-cauldron opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
#: mageia-cauldron
msgid "B<grub-render-label> renders an Apple disk label (.disk_label) file."
msgstr ""
"B<grub-render-label> Erzeugt eine Apple-Plattenbezeichnungsdatei (."
"disk_label)."

#. type: SH
#: mageia-cauldron
#, no-wrap
msgid "OPTIONS"
msgstr "OPTIONEN"

#. type: TP
#: mageia-cauldron
#, no-wrap
msgid "B<--color>=I<COLOR>"
msgstr "B<--color>=I<FARBE>"

# FIXME Bad formatting
#. type: Plain text
#: mageia-cauldron
msgid "Use I<COLOR as the color for generated labels.>"
msgstr "verwendet die angegebene I<FARBE> für die erzeugten Bezeichnungen."

#. type: TP
#: mageia-cauldron
#, no-wrap
msgid "B<--bgcolor>=I<COLOR>"
msgstr "B<--bgcolor>=I<FARBE>"

#. type: Plain text
#: mageia-cauldron
msgid "Use I<COLOR> as the background color for generated labels."
msgstr ""
"verwendet die angegebene I<FARBE> als Hintergrundfarbe für die erzeugten "
"Bezeichnungen."

#. type: TP
#: mageia-cauldron
#, no-wrap
msgid "B<--font>=I<FILE>"
msgstr "B<--font>=I<DATEI>"

#. type: Plain text
#: mageia-cauldron
msgid "Use I<FILE> as the font file for generated labels."
msgstr ""
"verwendet die angegebene I<SCHRIFT> als die Schriftdatei für die erzeugten "
"Bezeichnungen."

#. type: TP
#: mageia-cauldron
#, no-wrap
msgid "--input=I<FILE>"
msgstr "--input=I<DATEI>"

#. type: Plain text
#: mageia-cauldron
msgid "Read input text from I<FILE>."
msgstr "liest den Text aus der angegebenen I<DATEI>."

#. type: TP
#: mageia-cauldron
#, no-wrap
msgid "--output=I<FILE>"
msgstr "--output=I<DATEI>"

#. type: Plain text
#: mageia-cauldron
msgid "Render output to I<FILE>."
msgstr "schreibt die Ausgabe in die angegebene I<DATEI>."

#. type: TP
#: mageia-cauldron
#, no-wrap
msgid "--text=I<STRING>"
msgstr "--text=I<ZEICHENKETTE>"

#. type: Plain text
#: mageia-cauldron
msgid "Use I<STRING> as input text."
msgstr "verwendet die angegebene I<ZEICHENKETTE> als Eingabetext."

#. type: TP
#: mageia-cauldron
#, no-wrap
msgid "--verbose"
msgstr "--verbose"

#. type: Plain text
#: mageia-cauldron
msgid "Print verbose output."
msgstr "gibt ausführliche Meldungen aus."

#. type: SH
#: mageia-cauldron opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "SEE ALSO"
msgstr "SIEHE AUCH"

#. type: Plain text
#: mageia-cauldron
msgid "B<info grub>"
msgstr "B<info grub>"

#. type: TH
#: opensuse-leap-15-3
#, no-wrap
msgid "April 2021"
msgstr "April 2021"

#. type: TH
#: opensuse-leap-15-3
#, no-wrap
msgid "grub-render-label (GRUB2) 2.04"
msgstr "grub-render-label (GRUB2) 2.04"

#. type: TH
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "User Commands"
msgstr "Dienstprogramme für Benutzer"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "grub-render-label - generate a .disk_label for Apple Macs."
msgstr "grub-render-label - eine .disk_label-Datei für Apple Macs erzeugen."

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "B<grub-render-label> [I<\\,OPTION\\/>...] [I<\\,OPTIONS\\/>]"
msgstr "B<grub-render-label> [I<\\,OPTION\\/>…] [I<\\,OPTIONEN\\/>]"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "Render Apple .disk_label."
msgstr "Erzeugt Apple-.disk_label."

#. type: TP
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "B<-b>, B<--bgcolor>=I<\\,COLOR\\/>"
msgstr "B<-b>, B<--bgcolor>=I<\\,FARBE\\/>"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "use COLOR for background"
msgstr "verwendet die angegebene FARBE für den Hintergrund."

#. type: TP
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "B<-c>, B<--color>=I<\\,COLOR\\/>"
msgstr "B<-c>, B<--color>=I<\\,FARBE\\/>"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "use COLOR for text"
msgstr "verwendet die angegebene FARBE für Text."

#. type: TP
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "B<-f>, B<--font>=I<\\,FILE\\/>"
msgstr "B<-f>, B<--font>=I<\\,DATEI\\/>"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "use FILE as font (PF2)."
msgstr "verwendet die angegebene DATEI als Schrift (PF2)."

#. type: TP
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "B<-i>, B<--input>=I<\\,FILE\\/>"
msgstr "B<-i>, B<--input>=I<\\,DATEI\\/>"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "read text from FILE."
msgstr "liest den Text aus der angegebenen DATEI."

#. type: TP
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "B<-o>, B<--output>=I<\\,FILE\\/>"
msgstr "B<-o>, B<--output>=I<\\,DATEI\\/>"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "set output filename. Default is STDOUT"
msgstr ""
"legt den Namen der Ausgabedatei fest. Per Vorgabe wird in die "
"Standardausgabe geschrieben."

#. type: TP
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "B<-t>, B<--text>=I<\\,STRING\\/>"
msgstr "B<-t>, B<--text>=I<\\,ZEICHENKETTE\\/>"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "set the label to render"
msgstr "gibt die zu erzeugende Bezeichnung an."

#. type: TP
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "B<-v>, B<--verbose>"
msgstr "B<-v>, B<--verbose>"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "print verbose messages."
msgstr "gibt ausführliche Meldungen aus."

#. type: TP
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "-?, B<--help>"
msgstr "B<-?>, B<--help>"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "give this help list"
msgstr "gibt eine kurze Hilfe aus."

#. type: TP
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "B<--usage>"
msgstr "B<--usage>"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "give a short usage message"
msgstr "gibt eine kurze Meldung zur Verwendung aus."

#. type: TP
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "B<-V>, B<--version>"
msgstr "B<-V>, B<--version>"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "print program version"
msgstr "gibt die Programmversion aus."

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid ""
"Mandatory or optional arguments to long options are also mandatory or "
"optional for any corresponding short options."
msgstr ""
"Erforderliche oder optionale Argumente für lange Optionen sind ebenso "
"erforderlich bzw. optional für die entsprechenden Kurzoptionen."

#. type: SH
#: opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "REPORTING BUGS"
msgstr "FEHLER MELDEN"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "Report bugs to E<lt>bug-grub@gnu.orgE<gt>."
msgstr "Melden Sie Fehler (auf Englisch) an E<lt>bug-grub@gnu.orgE<gt>."

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid ""
"The full documentation for B<grub-render-label> is maintained as a Texinfo "
"manual.  If the B<info> and B<grub-render-label> programs are properly "
"installed at your site, the command"
msgstr ""
"Die vollständige Dokumentation für B<grub-render-label> wird als ein Texinfo-"
"Handbuch gepflegt. Wenn die Programme B<info> und B<grub-render-label> auf "
"Ihrem Rechner ordnungsgemäß installiert sind, können Sie mit dem Befehl"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "B<info grub-render-label>"
msgstr "B<info grub-render-label>"

#. type: Plain text
#: opensuse-leap-15-3 opensuse-tumbleweed
msgid "should give you access to the complete manual."
msgstr "auf das vollständige Handbuch zugreifen."

