#. type: TH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "GRUB-MKFONT"
msgstr "GRUB-MKFONT"

#. type: TH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "Wed Feb 26 2014"
msgstr "Mittwoch, 26. Februar 2014"

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "NAME"
msgstr "BEZEICHNUNG"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"B<grub-mkfont> \\(em Convert common font file formats into the PF2 format."
msgstr ""
"B<grub-mkfont> \\(em übliche Schriftdateiformate in das PF2-Format umwandeln"

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "SYNOPSIS"
msgstr "ÜBERSICHT"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "B<grub-mkfont> [--ascii-bitmaps] [-a | --force-autohint]"
msgstr "B<grub-mkfont> [--ascii-bitmaps] [-a | --force-autohint]"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[-b | --bold] [-c | --asce=I<NUM>] [-d | --desc=I<NUM>]"
msgstr "[-b | --bold] [-c | --asce=I<ZAHL>] [-d | --desc=I<ZAHL>]"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[-i | --index=I<NUM>] [-n | --name=I<NAME>] [--no-bitmap]"
msgstr "[-i | --index=I<ZAHL>] [-n | --name=I<NAME>] [--no-bitmap]"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[--no-hinting] E<lt>-o | --output=I<FILE>E<gt>"
msgstr "[--no-hinting] E<lt>-o | --output=I<DATEI>E<gt>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[-r | --range=I<FROM-TO>[I<,FROM-TO>]] [-s | --size=I<SIZE>]"
msgstr "[-r | --range=I<VON-BIS>[I<,VON-BIS>]] [-s | --size=I<GRÖSSE>]"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[-v | --verbose] [--width-spec] I<FONT_FILES>"
msgstr "[-v | --verbose] [--width-spec] I<SCHRIFTDATEIEN>"

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "DESCRIPTION"
msgstr "BESCHREIBUNG"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"B<grub-mkfont> converts font files from common formats into the PF2 format "
"used by GRUB."
msgstr ""
"B<grub-mkfont> wandelt Schriftdateien aus üblichen Formaten in das von GRUB "
"verwendete PF2-Format um."

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "OPTIONS"
msgstr "OPTIONEN"

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--ascii-bitmaps"
msgstr "--ascii-bitmaps"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Save only bitmaps for ASCII characters."
msgstr "speichert nur Bitmaps für ASCII-Zeichen."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--force-autohint"
msgstr "--force-autohint"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Force generation of automatic hinting."
msgstr "erzwingt die automatische Kantenglättung (Hinting)."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--bold"
msgstr "--bold"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Convert font to bold."
msgstr "wandelt in eine Fettschrift um."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--asce=I<NUM>"
msgstr "--asce=I<ZAHL>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Set font ascent to I<NUM>."
msgstr "stellt die Oberlänge auf die angegebene <ZAHL>."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--desc=I<NUM>"
msgstr "--desc=I<ZAHL>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Set font descent to I<NUM>."
msgstr "stellt die Unterlänge auf die angegebene <ZAHL>."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--index=I<NUM>"
msgstr "--index=I<ZAHL>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Select face index I<NUM>."
msgstr "wählt den Schriftart-Index <ZAHL>."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--name=I<NAME>"
msgstr "--name=I<NAME>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Set font family to I<NAME>."
msgstr "setzt den Namen der Schriftfamilie auf <NAME>."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--no-bitmap"
msgstr "--no-bitmap"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Ignore bitmap strikes when loading."
msgstr "ignoriert Bitmap-Schriftgrößen beim Laden."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--no-hinting"
msgstr "--no-hinting"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Disable hinting."
msgstr "deaktiviert die Kantenglättung (Hinting)."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--output=I<FILE>"
msgstr "--output=I<DATEI>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Save ouptut to I<FILE>.  This argument is required."
msgstr ""
"speichert die Ausgabe in der angegebenen I<DATEI> (muss angegeben werden)."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--range=I<FROM-TO>I<,FROM-TO>"
msgstr "--range=I<VON-BIS>I<,VON-BIS>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Set the font ranges to each pair of I<FROM>,I<TO>."
msgstr "setzt den Schriftbereich auf jedes I<VON-BIS>-Paar."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--size=I<SIZE>"
msgstr "--size=I<GRÖSSE>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Set font size to I<SIZE>."
msgstr "setzt die Schriftgröße auf I<GRÖSSE>."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--verbose"
msgstr "--verbose"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Print verbose messages."
msgstr "gibt ausführliche Meldungen aus."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--width-spec"
msgstr "--width-spec"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Create a width summary file."
msgstr "erstellt eine Zusammenfassungsdatei der Breite."

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "I<FONT_FILES>"
msgstr "I<SCHRIFTDATEIEN>"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "The input files to be converted."
msgstr "gibt die umzuwandelnden Eingabedateien an."

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "SEE ALSO"
msgstr "SIEHE AUCH"

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "B<info grub>"
msgstr "B<info grub>"
