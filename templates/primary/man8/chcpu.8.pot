#. type: Plain text
msgid "success"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<1>"
msgstr ""

#. type: Plain text
msgid "failure"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<64>"
msgstr ""

#. type: Plain text
msgid "partial success"
msgstr ""

