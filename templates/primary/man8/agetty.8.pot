#. type: TH
#, no-wrap
msgid "AGETTY"
msgstr ""

#. type: TH
#, no-wrap
msgid "February 2016"
msgstr ""

#. type: TH
#, no-wrap
msgid "util-linux"
msgstr ""

#. type: TH
#, no-wrap
