#, no-wrap
msgid "B<char *crypt(const char *>I<key>B<, const char *>I<salt>B<);>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "B<#define _GNU_SOURCE>         /* See feature_test_macros(7) */\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "B<#include E<lt>crypt.hE<gt>>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
