msgstr ""

#. type: Plain text
msgid "getdtablesize - get file descriptor table size"
msgstr ""

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
msgid "B<#include E<lt>unistd.hE<gt>>"
msgstr ""

#. type: Plain text
msgid "B<int getdtablesize(void);>"
msgstr ""

#. type: Plain text
