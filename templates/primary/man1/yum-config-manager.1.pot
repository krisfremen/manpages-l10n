msgid "yum-config-manager"
msgstr ""

#. type: TH
#, no-wrap
msgid "13 January 2013"
msgstr ""

#. type: SH
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
msgid "yum-config-manager - manage yum configuration options and yum repositories"
msgstr ""

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
msgid "B<yum-config-manager> [options]"
msgstr ""

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
msgid ""
"B<yum-config-manager> is a program that can manage main yum configuration "
"options, toggle which repositories are enabled or disabled, and add new "
"repositories."
msgstr ""

#. type: SH
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: IP
#, no-wrap
msgid "B<-h, --help>"
msgstr ""

#. type: Plain text
msgid "Display a help message, and then quit."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--save>"
msgstr ""

#. type: Plain text
msgid "Save the current options (useful with --setopt)."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--enable>"
msgstr ""

#. type: Plain text
msgid "Enable the specified repos (automatically saves)."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--disable>"
msgstr ""

#. type: Plain text
msgid "Disable the specified repos (automatically saves)."
msgstr ""

#. type: IP
#, no-wrap
msgid "B<--add-repo=ADDREPO>"
msgstr ""

#. type: Plain text
msgid "Add (and enable) the repo from the specified file or url."
msgstr ""

#. type: SH
#, no-wrap
msgid "ADDITIONAL OPTIONS"
msgstr ""

#. type: Plain text
msgid ""
"Yum-config-manager inherits all other options from yum. See the yum(8)  man "
"page for more information."
msgstr ""

#. type: SH
#, no-wrap
msgid "SEE ALSO"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"I<yum.conf (5)>\n"
"http://yum.baseurl.org/\n"
msgstr ""

