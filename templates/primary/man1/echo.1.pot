#. type: SH
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
msgid "echo - display a line of text"
msgstr ""

#. type: SH
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
msgid "B<echo> [I<\\,SHORT-OPTION\\/>]... [I<\\,STRING\\/>]..."
msgstr ""

#. type: Plain text
msgid "B<echo> I<\\,LONG-OPTION\\/>"
msgstr ""

#. type: SH
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
msgid "Echo the STRING(s) to standard output."
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-n>"
msgstr ""

#. type: Plain text
msgid "do not output the trailing newline"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-e>"
msgstr ""

#. type: Plain text
msgid "enable interpretation of backslash escapes"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<-E>"
msgstr ""

#. type: Plain text
msgid "disable interpretation of backslash escapes (default)"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
msgid "display this help and exit"
msgstr ""

#. type: TP
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
msgid "output version information and exit"
msgstr ""

#. type: Plain text
msgid "If B<-e> is in effect, the following sequences are recognized:"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\e\\e"
msgstr ""

#. type: Plain text
msgid "backslash"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\ea"
msgstr ""

#. type: Plain text
msgid "alert (BEL)"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\eb"
msgstr ""

#. type: Plain text
msgid "backspace"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\ec"
msgstr ""

#. type: Plain text
msgid "produce no further output"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\ee"
msgstr ""

#. type: Plain text
msgid "escape"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\ef"
msgstr ""

#. type: Plain text
msgid "form feed"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\en"
msgstr ""

#. type: Plain text
msgid "new line"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\er"
msgstr ""

#. type: Plain text
msgid "carriage return"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\et"
msgstr ""

#. type: Plain text
msgid "horizontal tab"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\ev"
msgstr ""

#. type: Plain text
msgid "vertical tab"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\e0NNN"
msgstr ""

#. type: Plain text
msgid "byte with octal value NNN (1 to 3 digits)"
msgstr ""

#. type: TP
#, no-wrap
msgid "\\exHH"
msgstr ""

#. type: Plain text
msgid "byte with hexadecimal value HH (1 to 2 digits)"
msgstr ""

