#: debian-bullseye debian-unstable
#, no-wrap
msgid "systemd 247"
msgstr ""

#. type: Plain text
#: debian-bullseye debian-unstable
msgid "/lib/systemd/systemd-modules-load"
msgstr ""

#. type: TH
