msgstr ""

#. type: SH
#: archlinux fedora-rawhide mageia-cauldron
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: archlinux fedora-rawhide mageia-cauldron
msgid "systemd-homed\\&.service"
msgstr ""

#. type: Plain text
#: archlinux fedora-rawhide mageia-cauldron
