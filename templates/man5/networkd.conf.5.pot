msgid "networkd.conf, networkd.conf.d - Global Network configuration files"
msgstr ""

#. type: SH
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "/etc/systemd/networkd\\&.conf"
msgstr ""

#. type: Plain text
