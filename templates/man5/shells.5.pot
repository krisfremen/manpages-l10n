#: archlinux debian-unstable mageia-cauldron
#, no-wrap
msgid "EXAMPLE"
msgstr ""

#. type: Plain text
#: archlinux debian-unstable mageia-cauldron
msgid "I</etc/shells> may contain the following paths:"
msgstr ""

#. type: Plain text
#: archlinux debian-unstable mageia-cauldron
#, no-wrap
msgid ""
"I</bin/sh>\n"
"I</bin/bash>\n"
"I</bin/csh>\n"
msgstr ""

