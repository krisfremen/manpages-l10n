msgstr ""

#. type: TH
#: archlinux debian-unstable
#, no-wrap
msgid "2017-09-15"
msgstr ""

#. type: TH
#: archlinux debian-unstable
#, no-wrap
msgid "Linux"
msgstr ""

#. type: TH
#: archlinux debian-unstable
