msgid "EXAMPLE"
msgstr ""

#. type: Plain text
#: archlinux debian-stretch debian-unstable
msgid "Check the condition of DTR on the serial port."
msgstr ""

#. type: Plain text
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid ""
"#include E<lt>termios.hE<gt>\n"
"#include E<lt>fcntl.hE<gt>\n"
"#include E<lt>sys/ioctl.hE<gt>\n"
msgstr ""

#. type: Plain text
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid ""
"int\n"
"main(void)\n"
"{\n"
"    int fd, serial;\n"
msgstr ""

#. type: Plain text
#: archlinux debian-stretch debian-unstable
#, no-wrap
msgid ""
"    fd = open(\"/dev/ttyS0\", O_RDONLY);\n"
"    ioctl(fd, TIOCMGET, &serial);\n"
"    if (serial & TIOCM_DTR)\n"
"        puts(\"TIOCM_DTR is set\");\n"
"    else\n"
"        puts(\"TIOCM_DTR is not set\");\n"
"    close(fd);\n"
"}\n"
msgstr ""

