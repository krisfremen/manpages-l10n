#. type: SH
#: fedora-rawhide mageia-cauldron opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "B<grub-mklayout> \\(em Generate a GRUB keyboard layout file."
msgstr ""

#. type: SH
#: fedora-rawhide mageia-cauldron opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "B<grub-mklayout> [-i | --input=I<FILE>] [-o | --output=I<FILE>]"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "[-v | --verbose]"
msgstr ""

#. type: SH
#: fedora-rawhide mageia-cauldron opensuse-leap-15-3 opensuse-tumbleweed
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"B<grub-mklayout> generates a GRUB keyboard layout description which "
"corresponds with the Linux console layout description given as input."
msgstr ""

#. type: SH
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--input=I<FILE>"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"Use I<FILE> as the input.  The default value is the standard input device."
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--output=I<FILE>"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid ""
"Use I<FILE> as the output.  The default value is the standard output device."
msgstr ""

#. type: TP
#: fedora-rawhide mageia-cauldron
#, no-wrap
msgid "--verbose"
msgstr ""

#. type: Plain text
#: fedora-rawhide mageia-cauldron
msgid "Print verbose messages."
msgstr ""

